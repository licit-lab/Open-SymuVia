<?xml version="1.0" encoding="utf-8"?>
<!--26/08/14: add ROUTES node -->
<!--25/09/14: add FLUX_GLOBAL and FLUX_TYPEVEHS nodes for describing OD matrices -->
<xsd:schema version="1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" vc:minVersion="0"
  xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning">
  <xsd:simpleType name="typeFloatStrictPositif">
    <xsd:restriction base="xsd:float">
      <xsd:minExclusive value="0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeFloatPositif">
    <xsd:restriction base="xsd:float">
      <xsd:minInclusive value="0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeFloatNegatif">
    <xsd:restriction base="xsd:float">
      <xsd:maxInclusive value="0"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeFloat01">
    <xsd:annotation>
      <xsd:documentation>Réel compris entre 0 et 1</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:float">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeIntStrictPositif">
    <xsd:restriction base="xsd:positiveInteger">
      <xsd:minInclusive value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeFloat01List">
    <xsd:restriction>
      <xsd:simpleType>
        <xsd:list itemType="typeFloat01"/>
      </xsd:simpleType>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeFloat01NormedList">
    <xsd:annotation>
      <xsd:documentation>La somme des coefficients doit être égale à 1</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction>
      <xsd:simpleType>
        <xsd:list itemType="typeFloat01"/>
      </xsd:simpleType>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeTouteLaSimulation">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="always"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeDuree">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="typeVitesses">
    <xsd:union memberTypes="typeFloatStrictPositif"/>
  </xsd:simpleType>
  <xsd:simpleType name="typeInfini">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="infini"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeVitMax">
    <xsd:annotation/>
    <xsd:union memberTypes="typeFloatStrictPositif typeInfini"/>
  </xsd:simpleType>
  <xsd:simpleType name="typeCoordonnees">
    <xsd:restriction>
      <xsd:simpleType>
        <xsd:list itemType="xsd:float"/>
      </xsd:simpleType>
      <xsd:minLength value="2"/>
      <xsd:maxLength value="3"/>
      <xsd:whiteSpace value="collapse"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="typeRGBAColor">
    <xsd:restriction base="typeFloat01List">
      <xsd:minLength value="3"/>
      <xsd:maxLength value="4"/>
      <xsd:whiteSpace value="collapse"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="typeAny" mixed="false">
    <xsd:sequence minOccurs="0">
      <xsd:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:anyAttribute namespace="##any" processContents="skip"/>
  </xsd:complexType>
  <xsd:complexType name="typePointsInternes">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="POINT_INTERNE">
        <xsd:annotation>
          <xsd:documentation>
                  Description d'un point interne du tronçon
                </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="coordonnees" type="typeCoordonnees" use="required">
            <xsd:annotation>
              <xsd:documentation>Coordonnées géographiques du point interne</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="typeString256">
    <xsd:restriction>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string"/>
      </xsd:simpleType>
      <xsd:maxLength value="256"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="typeCoeffDir">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" name="COEFF_DIR">
        <xsd:annotation>
          <xsd:documentation>Description d'une variante temporelle des coefficients directionnels</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="COEFFS_TRONCON_AMONT">
              <xsd:annotation>
                <xsd:documentation>Collection des coefficients directionnels pour la liste des tronçons amont</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="COEFF_TRONCON_AMONT">
                    <xsd:annotation>
                      <xsd:documentation>Description des coefficients directionnels pour un tronçon amont</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="COEFFS_VOIE_AMONT">
                          <xsd:annotation>
                            <xsd:documentation>Collection des coefficients directionnels pour la liste des voies du tronçon amont</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element maxOccurs="unbounded" name="COEFF_VOIE_AMONT">
                                <xsd:annotation>
                                  <xsd:documentation>Description des coefficients directionnels pour un tronçon amont et une voie donnée</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element name="COEFFS_TRONCON_AVAL">
                                      <xsd:annotation>
                                        <xsd:documentation>Collection des coefficients directionnels des tronçons aval pour un tronçon et une voie données en amont. Si un tronçon aval du répartiteur parent ne se trouve pas dans la liste, cela signifie qu'aucun véhicule ou flux provenant de la voie du tronçon amont considéré ne sort du répartiteur par ce tronçon aval</xsd:documentation>
                                      </xsd:annotation>
                                      <xsd:complexType>
                                        <xsd:sequence>
                                          <xsd:element maxOccurs="unbounded"
                                            name="COEFF_TRONCON_AVAL">
                                            <xsd:annotation>
                                              <xsd:documentation>Collection des coefficients directionnels pour un tronçon amont, une voie donnée et un tronçon aval</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                              <xsd:attribute name="id_aval" type="xsd:string"
                                                use="required">
                                                <xsd:annotation>
                                                  <xsd:documentation>Identifiant unique du tronçon aval</xsd:documentation>
                                                </xsd:annotation>
                                              </xsd:attribute>
                                              <xsd:attribute name="coeffs" type="typeFloat01List"
                                                use="required">
                                                <xsd:annotation>
                                                  <xsd:documentation>Vecteur des coefficients directionnels des voies du tronçon aval pour le tronçon amont et la voie amont considérés. Les valeurs comprises entre 0 et 1 doivent être ordonnées selon la numérotation des voies (voie 1 la plus à droite dans le sens de la circulation). Si une seule valeur est donnée alors que le tronçon comporte plusieurs voies, le coefficient directionnel s'applique au tronçon aval entier, et SymuVia est responsable de la ventilation de ce coefficient sur les différentes voies du tronçon.</xsd:documentation>
                                                </xsd:annotation>
                                              </xsd:attribute>
                                            </xsd:complexType>
                                          </xsd:element>
                                        </xsd:sequence>
                                      </xsd:complexType>
                                    </xsd:element>
                                  </xsd:sequence>
                                  <xsd:attribute name="numvoie" type="typeIntStrictPositif"
                                    use="optional">
                                    <xsd:annotation>
                                      <xsd:documentation>Numéro de voie du tronçon amont considéré. Si cet attribut n'est pa renseigné, les coefficients directionnels définis s'appliquent à toutes les voies.</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:attribute>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute name="id_amont" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant du tronçon amont</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute default="always" name="duree" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en seconde) de la variante de répartition du flux</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeAffectation">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" name="AFFECTATION">
        <xsd:annotation>
          <xsd:documentation>Description d'une variante des données d'affectation de la connexion (ce noeud est défini uniquement si le comportement de flux est de type 'destination').</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="COUPLES_ENTREE_DESTINATION">
              <xsd:annotation>
                <xsd:documentation>Noeud d'accès à la collection des couples entrée / destination des données d'affectation</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="unbounded" name="COUPLE_ENTREE_DESTINATION">
                    <xsd:annotation>
                      <xsd:documentation>Description des données d'affectation d'un couple entrée / destination de la connexion</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element minOccurs="1" maxOccurs="1" name="SORTIES_CONNEXION">
                          <xsd:annotation>
                            <xsd:documentation>Noeud d'accès à la liste des sorties affectées</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="1" maxOccurs="unbounded"
                                name="SORTIE_CONNEXION">
                                <xsd:annotation>
                                  <xsd:documentation>Description de la sortie affectée</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:attribute name="id_troncon_aval" type="xsd:string"
                                    use="required">
                                    <xsd:annotation>
                                      <xsd:documentation>Identifiant du tronçon aval (sortie) de la connexion</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:attribute>
                                  <xsd:attribute default="1" name="coeff" type="typeFloat01"
                                    use="optional">
                                    <xsd:annotation>
                                      <xsd:documentation>Coefficient d'affectation de la sortie</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:attribute>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute name="id_troncon_amont" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant du tronçon amont (entrée) de la connexion</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="id_destination" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant de la destination</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute default="always" name="duree" type="typeDuree">
            <xsd:annotation>
              <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en seconde) de la variante de l'affectation</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeMouvementAutorise">
    <xsd:sequence>
      <xsd:element minOccurs="1" maxOccurs="unbounded" name="MOUVEMENT_AUTORISE">
        <xsd:annotation>
          <xsd:documentation>Description d'un mouvement de la connexion interne</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="MOUVEMENT_SORTIES">
              <xsd:annotation>
                <xsd:documentation>Description de l'aval du mouvement (si un tronçon aval de la connexion n'est pas présent, les véhicules entrant par le tronçon et la voie considérés ne peuvent pas sortir par ce tronçon)</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="unbounded" name="MOUVEMENT_SORTIE">
                    <xsd:annotation>
                      <xsd:documentation>Description de l'aval d'un mouvement</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:attribute name="id_troncon_aval" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant du tronçon aval</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute default="1" name="num_voie_aval" type="typeIntStrictPositif"
                        use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Numéro de la voie aval</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute default="1" name="taux_utilisation" type="typeFloat01"
                        use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Taux d'utilisation de cette voie aval parmi l'ensemble des voies du tronçon aval pour les mouvements issus de la voie amont considéré. Si plusieurs mouvements issus de la même voie arrivent sur un même tronçon aval, la somme des taux d'utilisation pour l'ensemble des voies de ce tronçon aval doit être égale à 1.</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="libelle" type="xsd:string">
                        <xsd:annotation>
                          <xsd:documentation>Libellé du mouvement</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="ligne_feu" type="xsd:float" use="optional" default="0">
                        <xsd:annotation>
                          <xsd:documentation>Position curviligne de la ligne de feu rattachée au mouvement. Cette position est définie  par rapport à l'extrémité de la voie amont du mouvement et sur la trajectoire définie par le mouvement.
A définir uniquuement si un contrôleur de feux est associé à la connexion.</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="ligne_feu_relative" type="xsd:boolean" use="optional"
                        default="false">
                        <xsd:annotation>
                          <xsd:documentation>Indique que la position de la ligne de feu est exprimée en pourcentage de la longueur du tronçon amont</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="vit_max" type="typeFloatStrictPositif" use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Vitesse maximale spécifique à ce mouvement autorisé (en m/s)</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="exclusion_types_vehicules" type="xsd:string">
                        <xsd:annotation>
                          <xsd:documentation>Liste des identifiants des types de véhicules qui sont exclus de ce mouvement autorisé</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="id_troncon_amont" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Identifiant du tronçon amont de la connexion interne</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="1" name="num_voie_amont" type="typeIntStrictPositif"
            use="optional">
            <xsd:annotation>
              <xsd:documentation>Numéro de la voie du tronçon amont considérée</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeConnexionInterne">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="1" name="MOUVEMENTS_AUTORISES"
        type="typeMouvementAutorise"/>
    </xsd:sequence>
    <xsd:attribute default="0" name="z_level_crossing" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Gestion des multi-niveaux des éléments du réseau (ponts, tunnels...): valeur du z-level
Dans le cas d'une brique de connexion, on suppose que l'ensemble des éléments la composant possède une unique valeur de z-level.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeTraficConnexionInterne">
    <xsd:sequence>
      <xsd:choice>
        <xsd:element minOccurs="0" maxOccurs="1" name="AFFECTATIONS" type="typeAffectation">
          <xsd:annotation>
            <xsd:documentation>Noeud d'accès à la description de l'affectation pour l'ensemble des véhicules</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" maxOccurs="1" name="AFFECTATIONS_TYPE_VEHICULE">
          <xsd:annotation>
            <xsd:documentation>Noeud d'accès à la description des affectations par type de véhicule</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element minOccurs="1" maxOccurs="unbounded" name="AFFECTATION_TYPE_VEHICULE">
                <xsd:annotation>
                  <xsd:documentation>Noeud d'accès à la description de l'affectation pour un type de véhicule</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element minOccurs="1" maxOccurs="1" name="AFFECTATIONS"
                      type="typeAffectation"/>
                  </xsd:sequence>
                  <xsd:attribute name="id_type_vehicule" type="xsd:string" use="required">
                    <xsd:annotation>
                      <xsd:documentation>Identifiant du type de véhicule décrit</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
      <xsd:sequence>
        <xsd:element minOccurs="0" maxOccurs="unbounded" name="COEFFS_DIR">
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="typeCoeffDir">
                <xsd:attribute name="type_veh" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>Type de véhicule concerné par les coefficients directionnels. Si cet attribut n'est pas renseigné, les coefficients directionnels s'appliquent à tous les types de véhicules.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:extension>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="1" name="LISTE_TEMPS_INSERTION">
        <xsd:annotation>
          <xsd:documentation>Liste des temps d'insertion sur la connexion pour tous les types de véhicule circulant sur le réseau</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="TEMPS_INSERTION">
              <xsd:annotation>
                <xsd:documentation>Temps d'insertion sur la connexion pour un type de véhicule</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="id_typevehicule" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du type de véhicule concerné</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="ti" type="typeFloatStrictPositif" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Temps (en seconde) d'insertion</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="tt" type="typeFloatStrictPositif" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Temps (en seconde) de traversée. Inutilisé pour les convergents.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'élément du réseau associé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="PeriodeAgregationCapteurs" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période (en s) d'agrégation des capteurs</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Gamma" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Rapport Gamma</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Mu" type="typeFloat01" use="optional">
      <xsd:annotation>
        <xsd:documentation>Coefficient de priorité</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Beta" type="typeFloat01" use="optional">
      <xsd:annotation>
        <xsd:documentation>Pour giratoires uniquement : probabilité pour un véhicule voulant s'insérer de détecter un véhicule sortant du giratoire en régime fluide (le véhicule sortant détecté ne gène pas l'insertion).</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="BetaInt" type="typeFloat01" use="optional">
      <xsd:annotation>
        <xsd:documentation>Pour giratoires uniquement : probabilité pour un véhicule voulant s'insérer d'être géné par un véhicule arrivant sur la voie interne du giratoire en régime fluide (à définir uniquement dans le cas d'un giratoire à 2 voies)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="pos_cpt_Av" type="typeFloatPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Pour convergents uniquement : Position (en mètre) du capteur sur le tronçon aval</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="true" name="traversees" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Permet d'activer où non la gestion des conflits de traversée au niveau de la connexion interne (uniquement pour les briques de connexion : CAF et giratoire) </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="routes_predefinies" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Si true, les appels à SymGetAlternatePathsFromNodeJSON utilisent les routes prédéfinies dans le noeud RESEAU/ROUTES plutôt que de calculer les plus courts chemins correspondants. Remarque : il suffit qu'une connexion interne active cet attribut pour que plus aucun calcul d'itinéraire ne soit effectué par la fonction SymGetAlternatePathsFromNodeJSON.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeParametrageSimulation">
    <xsd:sequence>
      <xsd:element minOccurs="1" name="RESTITUTION">
        <xsd:annotation>
          <xsd:documentation>Noeud regroupant l'ensemble des options de modulation du contenu des fichiers de sortie</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence minOccurs="0">
            <xsd:element minOccurs="0" name="PLAGES_EXTRACTION">
              <xsd:annotation>
                <xsd:documentation>Plages temporelles d'écriture des instants de la simulation. Sans plage d'extraction l'écriture est réalisée sur toute la simulation.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence maxOccurs="unbounded">
                  <xsd:element name="PLAGE_EXTRACTION">
                    <xsd:annotation>
                      <xsd:documentation>Plage temporelle d'écriture des instants de la simulation.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:attribute default="00:00:00" name="debut_extraction" type="xsd:time">
                        <xsd:annotation>
                          <xsd:documentation>Heure de début d'extraction des résultats de la simulation (Heures:Minutes:Secondes)</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute default="00:00:00" name="fin_extraction" type="xsd:time">
                        <xsd:annotation>
                          <xsd:documentation>Heure de fin d'extraction des résultats de la simulation (Heures:Minutes:Secondes)</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute default="false" name="affectation_sortie" type="xsd:boolean" use="optional">
            <xsd:annotation>
              <xsd:documentation>Indique si les informations issues du calcul d'affectation sont générées dans un fichier au format XML (exploitable par SymuPlayer)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="0" name="trace_route" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Indique si les itinéraires des véhicules sont tracés dans le fichier trafic. 
Si oui, le calcul de la distance parcourue dans la procédure de trace des trajectoire s'effectue de façon exacte sinon la procédure utilise la vitesse à chaque pas de temps pour en déduire la distance parcourue.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="normal" name="sortie" use="optional">
            <xsd:annotation>
              <xsd:documentation>Mode se sortie du trafic. Le mode 'light' sauvegarde uniquement les résultats utiles à SymuPlot et SymuPlayer (notamment le mode 'light' ne permet pas de lancer la simulation acoustique correspondante).</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:NMTOKEN">
                <xsd:enumeration value="normal"/>
                <xsd:enumeration value="light"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
          <xsd:attribute default="1" name="trajectoires" type="xsd:boolean" use="optional">
            <xsd:annotation>
              <xsd:documentation>Indique si les trajectoires sont inscrites dans le fichier de sortie</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="false" name="chgt_voie_debug" type="xsd:boolean"/>
          <xsd:attribute default="0" name="debug" type="xsd:boolean" use="optional">
            <xsd:annotation>
              <xsd:documentation>Création d'un fichier trace et de sorties supplémentaires dans le fichier trafic (leader, deltaN)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="0" name="debug_matrice_OD" type="xsd:boolean" use="optional">
            <xsd:annotation>
              <xsd:documentation>Création d'un fichier _debug.log contenant la trace des matrices OD ainsi que leurs différentes variations</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="0" name="debug_SAS" type="xsd:boolean" use="optional">
            <xsd:annotation>
              <xsd:documentation>Création d'un fichier trace des SAS pour chaque voie microscopique</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="0" name="vgp_debug" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Sortie dans le fichier Simulation.log des informations relatives à la gestion des VGP par les controleurs de feux.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="false" name="trace_stocks" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Indique si on doit sortir les informations sur le stock de stationnement au niveau des parkings et tronçons</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="false" name="csv" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Indique si on doit produire les fichiers de résultat au format CSV</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="tout" name="csv_type">
            <xsd:annotation>
              <xsd:documentation>Indique quelles données sont à sortir dans les fichiers CSV (trajectoires, capteurs, ou les deux)</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:NMTOKEN">
                <xsd:enumeration value="trajectoires"/>
                <xsd:enumeration value="capteurs"/>
                <xsd:enumeration value="tout"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
          <xsd:attribute name="types_vehicules" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation>Identifiants du ou des types de véhicules dont les trajectoires doivent être restituées. par défaut, les trajectoires des véhicules de tous les types sont restitués.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="0" name="EPSGoutput" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation>Code EPSG du systèmes de coordonnées géoréférencées des résultats restitués
(WGS84: 4326;
Lambert 93: 2154)
Si ce code n'est pas défini (ainsi que le code des données d'entrée - attribut EPSGinput), la restitution des résultats s'effectuera sans transformation.
</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="true" name="vehicules_capteur_extraction" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Indique si on doit extraire les listes de véhicules des capteurs ponctuels</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="true" name="sortie_logs" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Indique si on doit créer les fichiers de log (chargement et simulation) pour la simulation correspondante</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="false" name="sortie_logs_light" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Si activé (true), ce mode permet de ne faire apparaître que certaines informations dans le fichier Simulation.log. Les informations de type "avertissement" sont produites. Les informations sur le calcul des itinéraires ou les instants ne sont pas produites.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="false" name="sortie_gml" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Indique si on doit créer les fichiers de description du réseau, les trajectoires et états de feux au format GML</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="false" name="trace_regime" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Si activé (true), ce mode permet de faire apparaître un attribut "free" des Trajectoires produites dans le fichier de trafic XML, qui vaut 1 si le véhicule correspondant est dans un état fluide ou 0 si congestionné. Le régime indiqué ne prend en compte que la congestion due à un éventuel leader (et pas celle due aux feux tricolores par exemple)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="false" name="sortie_temps_parcours" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Si activé (true), ce mode permet de sortir les temps de parcours et vitesses moyennes au niveau des tronçons dans un fichier au format JSON.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="900" name="sortie_temps_parcours_periode"
            type="typeIntStrictPositif">
            <xsd:annotation>
              <xsd:documentation>Période d'agrégation en secondes des temps de parcours de la sortie JSON correspondant à l'attribut sortie_temps_parcours</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="ELEMENTS">
        <xsd:annotation>
          <xsd:documentation>Ensemble des éléments du réseau pour lesquels sont définis des paramètres de simulation spécifiques</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ELEMENT" type="typeSimulationElement" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Elément du réseau pour lequel sont définis des paramètres de simulation spécifiques</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:ID" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la section SIMULATION</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="pasdetemps" type="typeFloatStrictPositif" use="required">
      <xsd:annotation>
        <xsd:documentation>Pas de temps de simulation</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="debut" type="xsd:time" use="required">
      <xsd:annotation>
        <xsd:documentation>Heure de début de la simulation (Heures:Minutes:Secondes)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="fin" type="xsd:time" use="required">
      <xsd:annotation>
        <xsd:documentation>Heure de fin de la simulation (Heures:Minutes:Secondes)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="true" name="simulation_trafic" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si la simulation du trafic doit avoir lieu</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="simulation_acoustique" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si la simulation des émissions acoustiques doit avoir lieu</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="simulation_atmospherique" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si la simulation des émissions atmosphériques doit avoir lieu</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="C" name="type_sortieacoustique">
      <xsd:annotation>
        <xsd:documentation>Type de sortie acoustique</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="C">
            <xsd:annotation>
              <xsd:documentation>cellule acoustique</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="P">
            <xsd:annotation>
              <xsd:documentation>ponctuel</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="estimee" name="loipoursuite" use="optional">
      <xsd:annotation>
        <xsd:documentation>Indique la loi de poursuite utilisée</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="exacte">
            <xsd:annotation>
              <xsd:documentation>Loi de poursuite précédente avec calcul exacte de la vitesse du leader au pas de temps suivant</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="estimee">
            <xsd:annotation>
              <xsd:documentation>Nouvelle loi de poursuite avec calcul approchée de la vitesse du leader au pas de temps suivant</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="comportementflux" use="required">
      <xsd:annotation>
        <xsd:documentation>Comportement du flux</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="dir">
            <xsd:annotation>
              <xsd:documentation>directionnel</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="des">
            <xsd:annotation>
              <xsd:documentation>les véhicules connaissent leur destination</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="iti">
            <xsd:annotation>
              <xsd:documentation>les véhicules connaissent leur itinéraire</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="1" name="nombre_pluscourtchemin" type="typeIntStrictPositif"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Nombre de plus court chemin à considérer dans le cas d'un comportment de flux de type 'itineraire'</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="offre_aval_convergent_deltaN" type="xsd:boolean"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Indique si l'offre en aval des convergents du réseau est calculé à l'aide de la quantité deltaN ou non </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="calcul_tq_convergent" type="xsd:boolean" use="optional">
      <xsd:annotation>
        <xsd:documentation>Indique si le calcul d'insertion du convergent prend en compte tq</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="date" type="xsd:date" use="optional">
      <xsd:annotation>
        <xsd:documentation>Date de la simulation</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="titre" type="typeString256" use="optional">
      <xsd:annotation>
        <xsd:documentation>Titre de la simulation</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="proc_deceleration" type="xsd:boolean" use="optional">
      <xsd:annotation>
        <xsd:documentation>Indique si la procédure de décélération doit être lancée à la fin du calcul</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="3" name="proc_dec_taux" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Valeur du taux de décélération à prendre en compte dans la procédure de décélération</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="seed" type="xsd:unsignedInt">
      <xsd:annotation>
        <xsd:documentation>Graine du générateur aléatoire (deux exécutions avec le même seed donnent des résultats identiques).
La valeur par défaut 0 signifie que le 'seed' est évalué par le logiciel.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="statique" name="affectation" use="optional">
      <xsd:annotation>
        <xsd:documentation>Type d'affectation dans le cas d'un comportement de flux 'itineraire'</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="statique"/>
          <xsd:enumeration value="dynamique"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="900" name="periode_affectation" type="typeIntStrictPositif"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Période d'affectation dans le cas d'un comportement de flux de type 'affectation_calculee'. Elle est saisie sous la forme d'un coefficient multiplicateur entier du pas de temps. Afin d'estimer au mieux les temps de parcours de chaque tronçon, la période doit être suffisamment grande pour que plusieurs véhicules aient le temps de parcourir entièrement le tronçon.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="NE" name="affectation_equilibre" use="optional">
      <xsd:annotation>
        <xsd:documentation>Paramétrage de la gestion de l'équilibre d'affectation : sans recherche d'équilibre (NE), recherche d'équilibre sans mémoire (ENM), recherche d'équilibre par la méthode des moyennes successives(EMS) </xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="NE">
            <xsd:annotation>
              <xsd:documentation>pas de recherche d'équilibre (les coefficients d'affectation sont calculés une seule fois au début de la période d'agrégation et appliqués durant la période d'affectation suivante sans recherche d'équilibre</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="ENM">
            <xsd:annotation>
              <xsd:documentation>recherche d'équilibre sans mémoire (les coefficients d'affectation sont initialisés au début de la période d'agrégation puis appliqués durant toute la période. A la fin de la période, si l'équilibre entre temps de parcours désiré et simulé n'est pas obtenu, des nouveaux coefficients d'affectation sont calculés sans prendre en compte le résultat précédent</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="MSA">
            <xsd:annotation>
              <xsd:documentation>recherche d'équilibre par la méthode des moyennes successives (les coefficients d'affectation sont initialisés au début de la période d'agrégation puis appliqués durant toute la période. A la fin de la période, si l'équilibre entre temps de parcours désiré et simulé n'est pas obtenu, des nouveaux coefficients d'affectation sont calculésen utilsant la méthode des moyennes successives</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="R" name="affectation_type_temps_de_parcours" use="optional">
      <xsd:annotation>
        <xsd:documentation>Type de calcul du temps de parcours : réalisé (R) ou prédictif (P)</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="R">
            <xsd:annotation>
              <xsd:documentation>réalisé</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PN">
            <xsd:annotation>
              <xsd:documentation>prédictif 1 (Nico)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="PL">
            <xsd:annotation>
              <xsd:documentation>prédictif 2 (Ludo)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="1" name="affectation_variation_temps_de_parcours" type="typeFloat01"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Variation minimale du temps de parcours d'un tronçon entraînant le calcul des coefficients d'affectation des connexions en amont du tronçon</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="affectation_nb_connexion_amont" type="typeIntStrictPositif"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Nombre de niveau à remonter à partir d'un tronçon lors du calcul des coefficients d'affectation suite à une variation importante du temps de parcours du tronçon</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.01" name="affectation_teta_logit" type="typeFloatStrictPositif"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Paramètre teta du modèle logit de l'affectation</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="logit" name="affectation_mode">
      <xsd:annotation>
        <xsd:documentation>Module d'affectation : mode d'affectation</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="logit">
            <xsd:annotation>
              <xsd:documentation>logit</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="wardrop">
            <xsd:annotation>
              <xsd:documentation>wardrop</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="0.1" name="affectation_seuil_convergence" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Module d'affectation : seuil de convergence sur l'ensemble du réseau à atteindre lors d'une recherche d'équilibre</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="100" name="affectation_nb_it_max" type="typeIntStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Module d'affectation : nombre maximal d'itération à réaliser lors d'une recherche d'équilibre (si le seuil de convergence n'est pas atteind). Si le nombre maximal est atteind, les coefficients d'affectation calculés au cours de la dernière itération sont appliqués
</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_calculKSP" use="optional" default="Dijkstra">
      <xsd:annotation>
        <xsd:documentation>Indique le mode de calcul des k plus courts chemins</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="basique">
            <xsd:annotation>
              <xsd:documentation>Calcul de tous les chemins et sélection selon leur coût</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Dijkstra">
            <xsd:annotation>
              <xsd:documentation>Utilisation de la méthode de Dijkstra avec pénalisation</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="0.1" name="affectation_seuil_tempsdeparcours" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Seuil maximal d'écart des temps de parcours réalisés et prédits pour considérer un tronçon comme correctement affecté</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="H" name="resolution">
      <xsd:annotation>
        <xsd:documentation>Résolution par défaut des éléments du réseau associé</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="M">
            <xsd:annotation>
              <xsd:documentation>macroscopique</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="H">
            <xsd:annotation>
              <xsd:documentation>microscopique</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="E">
            <xsd:annotation>
              <xsd:documentation>mésoscopique</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="0" name="nb_cell_acoustique" type="xsd:nonNegativeInteger">
      <xsd:annotation>
        <xsd:documentation>Nombre de cellule(s) acoustique(s) par défaut des éléments du réseau</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="longueur_cell_acoustique" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Longueur (en mètre) désirée des cellules acoustiques par défaut des éléments du réseau. (pour chaque tronçon / tronçon interne, le nombre de cellules acoustiques sera calculée afin d'approcher au mieux la longueur de cellule désirée)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="simulation_sirane" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si le calcul des données nécessaires à Sirane (agrégation du temps passé par les véhicules dans chaque plage de vitesse) est demandé.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="3" name="nb_cell_sirane" type="xsd:positiveInteger">
      <xsd:annotation>
        <xsd:documentation>Nombre de cellules à créer par tronçon pour le calcul des données nécessaires à Sirane</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="10" name="min_longueur_cell_sirane" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Longueur minimum (en mètres) d'une cellule pour le calcul des données nécessaires à Sirane</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="300" name="periode_agregation_sirane" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période d'agrégation (en secondes) pour le calcul des données nécessaires à Sirane</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="extension_barycentre_sirane" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si la géométrie indiquée en sortie pour les tronçons doit inclure les barycentres des briques amont et aval des tronçons. Si true, le parametre nb_cell_sirane est ignoré et les tronçons ne sont constitués que d'une seule cellule.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="INF" name="zone_distance_terminaison_max" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Radius pour l'orientation des positions tirées en zone en mode distribué (inactif si on utilise des plaques)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="affectation_reroutage" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Permet d'activer ou non la reaffectation dynamique des véhicules déjà présents sur le réseau (ou en attente aux entrées)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="affectation_reroutage_dstmax" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Permet d'indiquer une distance minimale avant le point de choix pour la réaffectation dynamique d'un véhicule (si cette distance avant le point de choix est dépassée, la reaffectation dynamique ne prendra pas en compte les itinéraires dont le prochain tronçon est différent du tronçon précédemment choisi après le point de choix)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.01" name="affectation_wardrop_tolerance" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>En mode wardrop, pourcentage utilisé pour déterminer les plus courts chemins retenus : ceux dont le coût est strictement inférieur à (cout minimum) * (1 + affectation_wardrop_tolerance)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.05" name="affectation_alpha" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Pourcentage de pénalisation pour Dijkstra</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="20" name="affectation_beta" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Facteur multiplicatif de limitation du cout non pénalisé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="20" name="affectation_gamma" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Facteur multiplicatif de limitation du cout pénalisé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="aucune" name="affectation_heuristique">
      <xsd:annotation>
        <xsd:documentation>Facteur multiplicatif de limitation du cout pénalisé</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="aucune">
            <xsd:annotation>
              <xsd:documentation>Pas d'heuristique (dijkstra classique)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="distanceEuclidienne">
            <xsd:annotation>
              <xsd:documentation>Utilisation de l'algorithme A* (l'heuristique utilisée est la distance euclidienne entre un noeud et la destination)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="5" name="affectation_heuristique_gamma" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Facteur multiplicatif de l'heuristique pour le calcul du poids des noeuds pour le calcul des plus courts chemins.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1.3" name="affectation_astar_beta" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Facteur multiplicatif du terme relatif à la functional class des tronçons pour le calcul des itinéraires avec A* (donc si affectation_heuristique vaut distanceEuclidienne).</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="affectation_commonality_filter" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Active ou non le filtre des itinéraires calculés en fonction du commonality factor</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_commonality_alpha" type="xsd:float" default="0">
      <xsd:annotation>
        <xsd:documentation>Valeur maximum du commonality factor admissible pour les itinéraires calculés par le module d'affectation</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.5" name="affectation_commonality_beta" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Valeur du paramètre beta pour le calcul du commonality factor</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="affectation_commonality_gamma" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Valeur du paramètre gamma pour le calcul du commonality factor</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="EPSGinput" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Code EPSG du systèmes de coordonnées géoréférencées des données d'entrée
(WGS84: 4326;
Lambert 93: 2154)
Si ce code n'est pas défini (ainsi que le code de restitution - attribut EPSGoutput), la restitution des résultats s'effectuera sans transformation.
</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_penalite_non_prioritaire" type="typeFloatPositif" default="5">
      <xsd:annotation>
        <xsd:documentation>Pénalité ajoutée aux mouvements non prioritaires, en secondes. Cette pénalité est appliquée pour les connexions de type : convergent (pour les tronçons amont non prioritaires), giratoire (pour tous les mouvements), répartiteur (pour les tourne à gauche)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_penalite_tourne_a_droite" type="typeFloatPositif" default="1">
      <xsd:annotation>
        <xsd:documentation>Pénalité ajoutée aux mouvements tourne à droite, en secondes. Cette pénalité est appliquée pour les répartiteurs (pour les mouvements tourne à droite)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_facteur_penalite_tourne_a_gauche" type="typeFloatPositif"
      default="1.5">
      <xsd:annotation>
        <xsd:documentation>Facteur multiplicatif appliqué au cout des mouvements de type tourne à gauche des carrefours à feux.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_facteur_penalite_tourne_a_droite" type="typeFloatPositif"
      default="1.1">
      <xsd:annotation>
        <xsd:documentation>Facteur multiplicatif appliqué au cout des mouvements de type tourne à droite des carrefours à feux.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_angle_tout_droit" type="typeFloatPositif" default="30">
      <xsd:annotation>
        <xsd:documentation>Angle en degrés entre le tronçon amont et le tronçon aval d'un mouvement au delà duquel le mouvement est considéré comme un tourne à droite ou tourne à gauche.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="affectation_estimation_attente_feux" type="xsd:boolean" default="true">
      <xsd:annotation>
        <xsd:documentation>Active ou non l'estimation du temps d'attente moyen aux feux pour l'estimation des temps de parcours à vide des tuyaux amont aux feux.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="micro_types_vehicules" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Types de véhicules à suivre en mode micro (le reste étant suivi en mode meso). Si plusieurs types, ceux-ci doivent être séparés par un espace.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="micro_distance_aval" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Distance en aval des véhicules à suivre en mode micro pour laquelle la simulation doit être faite en mode micro. Quelle que soit cette distance, au moins 1 tronçon aval est simulé en mode micro.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="micro_distance_amont" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Distance en amont des véhicules à suivre en mode micro pour laquelle la simulation doit se faire en mode micro.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="micro_distance_cotes" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Distance sur les côtés des noeuds se trouvant sur l'itinéraire des véhicules à suivre en mode micro pour laquelle la simulation doit se faire en mode micro.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeParametrageTrafic">
    <xsd:sequence minOccurs="0">
      <xsd:element minOccurs="0" name="TRONCONS">
        <xsd:annotation>
          <xsd:documentation>Ensemble des tronçons pour lesquels sont définis des paramètres de trafic particuliers</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="unbounded">
            <xsd:element name="TRONCON" type="typeTraficTroncon">
              <xsd:annotation>
                <xsd:documentation>Tronçon pour lequel est défini des paramètres de trafic particuliers</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="LOIS_DE_POURSUITE">
        <xsd:annotation>
          <xsd:documentation>Lois de poursuite scriptées à utiliser</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="LOI_DE_POURSUITE" type="typeCarFollowing">
              <xsd:annotation>
                <xsd:documentation>Description d'une loi de poursuite à utiliser</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="TYPES_DE_VEHICULE" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Liste des types de véhicule</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="TYPE_DE_VEHICULE">
              <xsd:annotation>
                <xsd:documentation>Description d'un type de véhicule</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent mixed="false">
                  <xsd:extension base="typeTypeDeVehicule"/>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="EXTREMITES">
        <xsd:annotation>
          <xsd:documentation>Liste des extremités (entrées et sorties) du réseau pour lesquelles des paramètres de trafic spécifiques sont définis</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="EXTREMITE"
              type="typeTraficExtremite">
              <xsd:annotation>
                <xsd:documentation>Description des paramètres de trafic d'une extremité (entrée ou sortie)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="PARKINGS">
        <xsd:annotation>
          <xsd:documentation>Liste des parkings du réseau pour lesquels des paramètres de trafic spécifiques sont définis</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="PARKING" type="typeTraficParking">
              <xsd:annotation>
                <xsd:documentation>Description des paramètres de trafic d'un parking</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="ZONES_DE_TERMINAISON">
        <xsd:annotation>
          <xsd:documentation>Liste des zones de terminaison du réseau pour lesquelles des paramètres de trafic spécifiques sont définis</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ZONE_DE_TERMINAISON" maxOccurs="unbounded" minOccurs="0"
              type="typeTraficZone">
              <xsd:annotation>
                <xsd:documentation>Description des paramètres de trafic d'une zone de terminaison</xsd:documentation>
              </xsd:annotation>
              <xsd:key name="plaqueKey">
                <xsd:selector xpath="./PLAQUES/PLAQUE"/>
                <xsd:field xpath="@id"/>
              </xsd:key>
              <xsd:key name="tronconPlaqueKey">
                <xsd:selector xpath="./TRONCONS/TRONCON"/>
                <xsd:field xpath="@id"/>
              </xsd:key>
              <xsd:keyref name="PlaqueSectionKeyref" refer="tronconPlaqueKey">
                <xsd:selector xpath="./PLAQUES/PLAQUE/SECTIONS/SECTION"/>
                <xsd:field xpath="@troncon"/>
              </xsd:keyref>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="CONNEXIONS_INTERNES">
        <xsd:annotation>
          <xsd:documentation>Liste des connexions internes au réseau pour lesquelles des paramètres de trafic spécifiques sont définis</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CONNEXION_INTERNE" type="typeTraficConnexionInterne"
              maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Connexion interne pour laquelle sont définis des paramètres de trafic particuliers</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="PARAMETRAGE_CAPTEURS" type="typeParametrageCapteurs">
        <xsd:annotation>
          <xsd:documentation>Paramétrage des capteurs du réseau</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="CONTROLEURS_DE_FEUX">
        <xsd:annotation>
          <xsd:documentation>Liste des contrôleurs de feux
</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="CONTROLEUR_DE_FEUX"
              type="typeControleurDeFeux">
              <xsd:annotation>
                <xsd:documentation>Description d'un contrôleur de feux
</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="REGULATIONS">
        <xsd:annotation>
          <xsd:documentation>Liste des briques de régulation de trafic définies</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="REGULATION" type="typeRegulation">
              <xsd:annotation>
                <xsd:documentation>Définition d'une brique de régulation</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="MOTIFS">
        <xsd:annotation>
          <xsd:documentation>Liste des motifs de déplacement</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="MOTIF">
              <xsd:annotation>
                <xsd:documentation>Définition d'un motif de déplacement</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du motif (ex : D ou Domicile)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="description" type="xsd:string" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Description du motif</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="PARAMETRAGE_STATIONNEMENT"
        type="typeParametrageStationnementGeneral">
        <xsd:annotation>
          <xsd:documentation>Paramètres de stationnement.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:ID" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la section TRAFIC</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="accbornee" type="xsd:boolean" use="required">
      <xsd:annotation>
        <xsd:documentation>Simulation de l'accélération bornée </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="true" name="chgtvoie" type="xsd:boolean" use="optional">
      <xsd:annotation>
        <xsd:documentation>Simulation du changement de voie</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.5" name="chgtvoie_mandatory_falpha" type="xsd:float" use="optional">
      <xsd:annotation>
        <xsd:documentation>Valeur du rapport alpha/(1+alpha) dans le cas d'un changement de voie 'mandatory' liée à une réduction de voie uniquement</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="U" name="chgtvoie_mandatory_probabilite" use="optional">
      <xsd:annotation>
        <xsd:documentation>Type de probabilité dans le cas d'une changement de voie 'mandatory' : uniforme (U) ou distribué (D)</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="U">
            <xsd:annotation>
              <xsd:documentation>probabilité uniforme</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="D">
            <xsd:annotation>
              <xsd:documentation>probabilité distribuée</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="S" name="chgtvoie_mandatory_mode" use="optional">
      <xsd:annotation>
        <xsd:documentation>Mode de gestion des changements de voie 'mandatory' : synchrone (S) ou asynchrone (A)</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="S">
            <xsd:annotation>
              <xsd:documentation>mode synchrone</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="A">
            <xsd:annotation>
              <xsd:documentation>mode asynchrone</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="GD" name="chgtvoie_discr_ordre" use="optional">
      <xsd:annotation>
        <xsd:documentation>Ordre d'étude des voies possibles du changement de voie</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="GD">
            <xsd:annotation>
              <xsd:documentation>D'abord la voie de gauche puis la voie de droite</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="DG">
            <xsd:annotation>
              <xsd:documentation>D'abord la voie de droite puis la voie de gauche</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="F">
            <xsd:annotation>
              <xsd:documentation>Voie la plus favorable (autorisant la vitesse la plus importante)</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="1" name="coeffrelax" type="xsd:float" use="optional">
      <xsd:annotation>
        <xsd:documentation>Coefficient de relaxation du réseau</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="50" name="chgtvoie_dstfin" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Distance avant la fin du tronçon pour forcer un changement de voie d'un véhicule si il ne se trouve pas sur la bonne voie pour prendre la prochaine direction (en mètre)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="chgtvoie_dstfin_force" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Distance avant la fin du tronçon pour forcer un changement de voie d'un véhicule si il ne se trouve pas sur la bonne voie pour prendre la prochaine direction (en mètre), avec un phi arbitraire (attribut chgtvoie_dstfin_force_phi)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="chgtvoie_dstfin_force_phi" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Valeur du Phi à utiliser pour le calcul du changement de voie dans la zone définie par chgtvoie_dstfin_force</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="agressivite" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si la procédure d'agressivité des véhicules est appliquée</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="chgtvoie_ghost" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si le changement de voie gère modélise la transition d'une voie à l'autre (génération de ghost sur les voies)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="chgtvoie_ghost_durationMin" type="typeFloatPositif"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Minimum de la durée en seconde de l'existence des ghosts suite à une changement de voie</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="8" name="chgtvoie_ghost_durationMax" type="typeFloatPositif"
      use="optional">
      <xsd:annotation>
        <xsd:documentation>Maximum de la durée en seconde de l'existence des ghosts suite à une changement de voie</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="20" name="chgtvoie_ghost_lenghtBevel" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Dans le cas d'une réduction de voie, longueur utile en fin de tronçon pour effectuer la procédure de ghost (le véhicule sur la voie réduite s'arrêtera donc à longueur(troncon) - chgtvoie_ghost_lengthBevel )</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="30" name="PeriodeAgregationCapteurs" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période (en s) d'agrégation des capteurs</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="Gamma" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Rapport Gamma</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="Mu" type="typeFloat01" use="optional">
      <xsd:annotation>
        <xsd:documentation>Coefficient de priorité</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Beta" type="typeFloat01" use="optional" default="0.5">
      <xsd:annotation>
        <xsd:documentation>Pour giratoires uniquement : probabilité pour un véhicule voulant s'insérer de détecter un véhicule sortant du giratoire en régime fluide (le véhicule sortant détecté ne gène pas l'insertion).</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.3" name="BetaInt" type="typeFloat01" use="optional">
      <xsd:annotation>
        <xsd:documentation>Pour giratoires uniquement : probabilité pour un véhicule voulant s'insérer d'être géné par un véhicule arrivant sur la voie interne du giratoire en régime fluide (à définir uniquement dans le cas d'un giratoire à 2 voies)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="20" name="pos_cpt_Av" type="typeFloatPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Pour convergents uniquement : Position (en mètre) du capteur sur le tronçon aval</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ti" type="typeFloatPositif" use="optional" default="0">
      <xsd:annotation>
        <xsd:documentation>Temps (en seconde) d'insertion</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="tt" type="typeFloatPositif" use="optional" default="0">
      <xsd:annotation>
        <xsd:documentation>Temps (en seconde) de traversée</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="depassement" type="xsd:boolean" use="optional">
      <xsd:annotation>
        <xsd:documentation>Simulation du dépassement (par passage sur tronçon opposé)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="true" name="traversees" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Permet d'activer où non la gestion globale des conflits de traversée au niveau des briques de connexion et de la traversée des voies interdites </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="meso_nb_vehicules_chgt_voie" type="xsd:nonNegativeInteger">
      <xsd:annotation>
        <xsd:documentation>En mode mésoscopique, nombre de véhicules non prioritaires à considérer par voie pour le passage d'un noeud, si le mouvement correspondant n'est pas gêné par le mouvement des véhicules arrivés avant (cas d'un feu vert pour tourner à droite avec une voie réservée à ce mouvement par exemple). Le nombre de véhicules considérés est la valeur de ce paramètre multipliée par le nombre de voies - 1.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="mode_depassement_chgt_direction" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Permet l'activation d'un mode anti congestion qui permet aux véhicules bloqués en fin de tronçon a une voie par des leaders de les dépasser si le prochain tronçon du véhicule est différent du tronçon cible de ses leaders. L adistance définissant la "fin du tronçon" est paramètrable via l'attribut distance_depassement_chgt_direction</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="20" name="distance_depassement_chgt_direction" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Distance (en mètres) à la fin du tronçon à partir duquel on considère les véhicules éligibles au dépassement pour changement de direction (voir attribut mode_depassement_chgt_direction)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeTypeDeVehicule">
    <xsd:all>
      <xsd:element minOccurs="0" name="LOIS_DE_POURSUITE">
        <xsd:annotation>
          <xsd:documentation>Liste des lois de poursuite scriptée à utiliser pour ce type de véhicule. Par défaut, la loi de poursuite Newell est utilisée.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="LOI_DE_POURSUITE">
              <xsd:annotation>
                <xsd:documentation>Description d'une loi de poursuite à utiliser pour le type de véhicule</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" name="PARAMETRES" type="typeAny">
                    <xsd:annotation>
                      <xsd:documentation>Ensemble des paramètres de la loi de poursuite</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de la loi de poursuite à utiliser</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="1" name="coeff" type="typeFloat01">
                  <xsd:annotation>
                    <xsd:documentation>Proportion de véhicules du type considéré pour laquelle utiliser la loi de poursuite.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="ACCELERATION_PLAGES">
        <xsd:annotation>
          <xsd:documentation>Liste ordonnée des plages de l'accélération en fonction de la vitesse dans le cas d'une accélération non constante (ce noeud n'est pas lu dans le cas où la simulation a pour caractéristique "accélération non bornée")</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="ACCELERATION_PLAGE">
              <xsd:annotation>
                <xsd:documentation>Description d'une plage d'accélération</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="ax" type="typeFloatStrictPositif" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Accélération en m/s2</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="vit_sup" type="typeVitMax" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Vitesse (en m/s) de la borne supérieure de validité de l'accélération</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="SOURCES_ACOUSTIQUES">
        <xsd:annotation>
          <xsd:documentation>Liste des sources acoustiques du véhicule</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="SOURCE_ACOUSTIQUE">
              <xsd:annotation>
                <xsd:documentation>Description d'une source acoustique du type de véhicule parent</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute default="0" name="position" type="xsd:float" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Position de la source acoustique du véhicule (l'origine est l'avant du véhicule)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="id_DB_LoiEmission" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant base de donnée des lois d'émission de la source acoustique du type de véhicule</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du type de véhicule</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="w" type="typeFloatNegatif" use="required">
      <xsd:annotation>
        <xsd:documentation>W (en m/s, négatif par convention) d'un véhicule de ce type. Si les attributs 'w_dispersion', 'w_min' et 'w_max' sont également définis, lors de la création d'un véhicule de type, la valeur W sera calculée à l'aide d'une distribution normale de moyenne 'w' et de dispersion 'w' (la plage [w_min, w_max] permet d'exclure les valeurs tirées en dehors de cette plage), sinon 'w' est la valeur constante de W des véhicules de ce type.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="w_dispersion" type="typeVitesses" use="optional">
      <xsd:annotation>
        <xsd:documentation>Valeur de dispersion de W dans le cas d'une distribution normale pour chaque véhicule créé. ATTENTION : la valeur doit être positive puisque c'est une dispersion</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="w_min" type="typeFloatNegatif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Borne inférieure des valeurs issues de la distribution normale de W (si une valeur tirée est strictement inférieure à cette borne, le tirage est réitéré), cette valeur est négative comme W</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="w_max" type="typeFloatNegatif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Borne supérieure des valeurs issues de la distribution normale de W (si une valeur tirée est strictement supérieure à cette borne, le tirage est réitéré), cette valeur est négative comme W</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="kx" type="typeFloatStrictPositif" use="required">
      <xsd:annotation>
        <xsd:documentation>Concentration max par voie (en veh/m). Si les attributs 'inv_kx_dispersion', 'kx_min' et 'kx_max' sont également définis, lors de la création d'un véhicule de type, la concentration sera calculée comme l'inverse d'une valeur issue d'une distribution normale de moyenne '1/kx' et de dispersion 'inv_kx_dispersion' (la plage [kx_min, kx_max] permet d'exclure les valeurs tirées en dehors de cette plage), sinon 'kx' est la concentration constante des véhicules de ce type.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="inv_kx_dispersion" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Valeur de dispersion de l'inverse de la concentration dans le cas d'une distribution normale pour chaque véhicule créé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="kx_min" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Borne inférieure de concentration des inserves des valeurs issues de la distribution normale (si une valeur tirée est strictement inférieure à cette borne, le tirage est réitéré)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="kx_max" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Borne supérieure de concentration des inverses des valeurs issues de la distribution normale (si une valeur tirée est strictement supérieure à cette borne, le tirage est réitéré)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="vx" type="typeVitesses" use="required">
      <xsd:annotation>
        <xsd:documentation>Vitesse libre (en m/s) d'un véhicule de ce type. Si les attributs 'vx_dispersion', 'vx_min' et 'vx_max' sont également définis, lors de la création d'un véhicule de type, la vitesse libre sera calculée à l'aide d'une distribution normale de moyenne 'vx' et de dispersion 'vx_dispersion' (la plage [vx_min, vx_max] permet d'exclure les valeurs tirées en dehors de cette plage), sinon 'vx' est la vitesse libre constante des véhicules de ce type.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="vx_dispersion" type="typeVitesses" use="optional">
      <xsd:annotation>
        <xsd:documentation>Valeur de dispersion de la vitesse libre dans le cas d'une distribution normale pour chaque véhicule créé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="vx_min" type="typeVitesses" use="optional">
      <xsd:annotation>
        <xsd:documentation>Borne inférieure des valeurs issues de la distribution normale de la vitesse libre (si une valeur tirée est strictement inférieure à cette borne, le tirage est réitéré)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="vx_max" type="typeVitesses" use="optional">
      <xsd:annotation>
        <xsd:documentation>Borne supérieure des valeurs issues de la distribution normale de la vitesse libre (si une valeur tirée est strictement supérieure à cette borne, le tirage est réitéré)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.5" name="ax" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Accélération maximale (en m/s2) dans le cas d'une accélération constante quelle que soit la vitesse (cet attribut n'est pas utilisé dans le cas où la simulation a pour caractéristique "accélération non bornée"</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="deceleration" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Décélération (en m/s2) du véhicule.
Pour un véhicule autre qu'un VGP, la décélération est modélisée uniquement pour le premier véhicule à l'approche d'un feu rouge (ou orange). Pour un VGP, la décélération est modélisée lors de la l'approche d'une feu rouge et d'un arrêt.
</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="deceleration_ecart_type" type="typeFloatPositif" use="optional" default="0">
      <xsd:annotation>
        <xsd:documentation>Ecart type de de la décélération à un feu pour la décélération stochastique. Si la valeur est 0 (défaut) alors la décélération n'est pas stochastique.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="espacement_arret" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Longueur en mètre de l'espace vide laissé par 2 véhicules successifs à l'arrêt (utilisé uniqement pour l'algorithme de traversée)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="agressivite" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Valeur d'agressivité du type de véhicule (limite inférieure du delta N du véhicule avant adaptation de sa vitesse par rapport à son leader)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.2" name="pi_rabattement" type="typeFloat01" use="optional">
      <xsd:annotation>
        <xsd:documentation>Valeur de Pi dans le cas d'un rabattement vers la voie la plus à droite des véhicules </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="4" name="chgtvoie_tau" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Caractéristique Tau du changement de voie</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="10" name="vitesse_laterale" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Vitesse de déplacement latérale (pour traversée de voies dans l'axe permendiculaire au tronçon, en m/s)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.1" name="depassement_tau" type="xsd:float" use="optional">
      <xsd:annotation>
        <xsd:documentation>Caractéristique Tau du dépassement sur tronçon opposé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="largeur" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>largeur du véhicule. Utilisé pour la représentation des véhicules dans SymuPlayer.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.1" name="proportion_agressive" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>Proportion des véhicules agressifs pour ce type de véhicule</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.1" name="proportion_timide" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>Proportion des véhicules timides pour ce type de véhicule</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1.1" name="eta_T_agressif" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>Valeur de eta T pour les véhicules agressifs</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.9" name="eta_T_timide" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>Valeur de eta T pour les véhicules timides</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.055" name="epsylon_0" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>Valeur absolue de epsylon 0 pour les véhicules agressifs et timides (en veh/s)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0.055" name="epsylon_1" type="xsd:double">
      <xsd:annotation>
        <xsd:documentation>Valeur absolue de epsylon 1 pour les véhicules agressifs et timides (en veh/s)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="car" name="citygml_type" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Nom du type de véhicule au sens CityGML (voir package CityGML "transportation"). Utilisé pour la production des sorties au format GML (cf. attribut sortie_gml). Si cet attribut n'est pas renseigné, le type "car" est utilisé par défaut.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeSimulationElement">
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'élément du réseau associé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="resolution">
      <xsd:annotation>
        <xsd:documentation>Résolution de l'élément du réseau associé</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="M">
            <xsd:annotation>
              <xsd:documentation>macroscopique</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="H">
            <xsd:annotation>
              <xsd:documentation>microscopique</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="E">
            <xsd:annotation>
              <xsd:documentation>mésoscopique</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="nb_cell_acoustique" type="xsd:nonNegativeInteger">
      <xsd:annotation>
        <xsd:documentation>Nombre de cellule(s) acoustique(s)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="longueur_cell_acoustique" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Longueur (en mètre) désirée des cellules acoustiques de l'élément. (pour chaque tronçon / tronçon interne, le nombre de cellules acoustiques sera calculée afin d'approcher au mieux la longueur de cellule désirée)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeTraficTroncon">
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du tronçon associé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="agressivite" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si les véhicules positionnés sur le tronçon peuvent avoir un comportement agressif</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="penalisation" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Coefficient de pénalisation du tronçon pour le caclul d'affectation. Le coût non pénalisé du tronçon est multiplié par ce coefficient dans les calculs. Un coefficient de 1 (valeur par défaut) n'a pas d'effet.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="func_class" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Functional Class du tronçon (notion navstreets : de 1 à 5 du type d'axe le plus important au moins important). Utilisé pour l'algorithme de recherche des plus courts chemins A* (c'est à dire si affectation_heuristique vaut distanceEuclidienne).</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeTroncon">
    <xsd:all>
      <xsd:element minOccurs="0" maxOccurs="1" name="VOIES_REDUITES">
        <xsd:annotation>
          <xsd:documentation>Liste des voies réduites pour les tronçons microscopiques(Symubruit force le changement de voie avant la fin du tronçon pour de telles voies)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="VOIE_REDUITE">
              <xsd:annotation>
                <xsd:documentation>Description d'une voie réduite</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="numvoie" type="typeIntStrictPositif" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Numéro de voie réduite</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="exclusion_types_vehicules" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>Liste des identifiants des types de véhicules pour lesquels la voie réduite ne s'applique pas</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="true" name="active" type="xsd:boolean">
                  <xsd:annotation>
                    <xsd:documentation>Indique si la voie réduite est active ou non (utile pour désactiver une voie réduite existante)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="VITESSES_REG">
        <xsd:annotation>
          <xsd:documentation>Liste des vitesses réglementaires spécifiques à un type de véhicule (à définir uniquement dans le cas d'un tronçon microscopique et lorsque la vitesse réglementaire n'est pas commune à tous les types de véhicules)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="1">
            <xsd:element maxOccurs="unbounded" name="VITESSE_REG" minOccurs="1">
              <xsd:annotation>
                <xsd:documentation>Description des vitesses réglementaires du tronçon pour une variante temporelle donnée</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="VITESSE_REG_PAR_TYPE">
                    <xsd:annotation>
                      <xsd:documentation>Description des vitesses réglementaires du tronçon pour une liste de types de véhicules donnée</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="VITESSE_REG_PAR_VOIE">
                          <xsd:annotation>
                            <xsd:documentation>Description de al vitesse réglementaire pour une voie et éventuellement une portion spaciale donnée</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:attribute name="numvoie" type="typeIntStrictPositif" use="optional">
                              <xsd:annotation>
                                <xsd:documentation>Numéro de voie concerné. Par défaut, toutes les voies le sont.</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="position_debut" type="typeFloatPositif" default="0"
                              use="optional">
                              <xsd:annotation>
                                <xsd:documentation>position curviligne à partir du troncon où commence la vitesse réglementaire</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="position_fin" type="typeFloatPositif"
                              use="optional" default="INF">
                              <xsd:annotation>
                                <xsd:documentation>position curviligne à partir du troncon où se termine la vitesse réglementaire</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="vitesse" type="typeFloatStrictPositif"
                              use="required">
                              <xsd:annotation>
                                <xsd:documentation>Vitesse réglementaire (en m/s)</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute name="types_vehicules" type="xsd:string" use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Identifiants du ou des types de véhicules concernés (ils doivent être définis dans la liste TYPES_DE_VEHICULE). Par défaut, tous les types de véhicules sont concernés.</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute default="always" name="duree" type="typeDuree">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en seconde) de la variante de vitesse</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="POINTS_INTERNES" type="typePointsInternes">
        <xsd:annotation>
          <xsd:documentation>
            Description de la liste des points internes décrivant le tronçon (ils doivent être définis de l'amont vers l'aval)
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="TRAVERSEES">
        <xsd:annotation>
          <xsd:documentation>Liste des traversées prioritaires pour les tronçons microscopiques</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="TRAVERSEE">
              <xsd:annotation>
                <xsd:documentation>Description d'une traversée prioritaire</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="1" name="LISTE_TEMPS_TRAVERSEE">
                    <xsd:annotation>
                      <xsd:documentation>Liste des temps de traversée du tronçon prioritaire pour tous les types de véhicule circulant sur le réseau</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element minOccurs="1" maxOccurs="unbounded" name="TEMPS_TRAVERSEE">
                          <xsd:annotation>
                            <xsd:documentation>Temps de traversée du tronçon prioritaire pour un type de véhicule</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:attribute name="id_typevehicule" type="xsd:string" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Identifiant du type de véhicule concerné</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="tf" type="typeFloatStrictPositif" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Temps (en seconde) de traversée</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="troncon" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Tronçon prioritaire traversant le tronçon parent (géométriquement, les deux tronçons doivent s'intersecter)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="VOIES_RESERVEES">
        <xsd:annotation>
          <xsd:documentation>Définition des voies réservées sur ce tronçon</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="1">
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="VOIE_RESERVEE">
              <xsd:annotation>
                <xsd:documentation>Définition d'une voie réservée</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="num_voie" type="xsd:positiveInteger" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Numéro de la voie réservée sur le tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="largeur_voie" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Largeur de la voie réservée</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="id_typesvehicules" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Liste des identifiants des types de véhicule pour lesquels la voie est réservée</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="always" name="duree" type="typeDuree">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en seconde) de la voie réservée. Par défaut, la voie est réservée pour toute la simulation.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="true" name="active" type="xsd:boolean">
                  <xsd:annotation>
                    <xsd:documentation>Indique si la voie réservée doit être activée ou désactivée</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="VOIES_INTERDITES">
        <xsd:annotation>
          <xsd:documentation>Définition des voies interdites sur le tronçon</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="VOIE_INTERDITE">
              <xsd:complexType>
                <xsd:attribute name="num_voie" type="xsd:positiveInteger" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Numéro de la voie interdite sur le tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="id_typesvehicules" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Liste des identifiants des types de véhicule pour lesquels la voie est interdite</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="TERRE_PLEINS">
        <xsd:annotation>
          <xsd:documentation>Ensemble des terre-pleins présents sur le tronçon</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="TERRE_PLEIN">
              <xsd:annotation>
                <xsd:documentation>Terre-plein présent sur le tronçon</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="TERRE_PLEIN_VARIATION" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Variation temporelle de la présence du terre-plein</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:attribute default="always" name="duree" type="typeDuree">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en seconde) de la présence du terre plein pour cette variation temporelle</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute default="true" name="actif" type="xsd:boolean">
                        <xsd:annotation>
                          <xsd:documentation>Indique si le terre-plein est actif ou non pendant la variation temporelle</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="num_voie" type="xsd:positiveInteger" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Numéro de la voie à gauche de laquelle se trouve le terre-plein</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="0" name="position_debut" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Position curviligne du début du terre-plein sur le tronçon (en m)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="INF" name="position_fin" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Position curviligne de la fin du terre-plein sur le tronçon (en m)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="ZONES_DEPASSEMENT_INTERDIT">
        <xsd:annotation>
          <xsd:documentation>Ensemble des zones dans lesquelles le dépassement par passage sur la voie de gauche du tronçon opposé est interdit </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ZONE_DEPASSEMENT_INTERDIT" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Zone dans laquelle le dépassement par passage sur la voie de gauche du tronçon opposé est interdit</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute default="0" name="position_debut" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Position curviligne du début de la zone de dépassement autorisée</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="INF" name="position_fin" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Position curviligne de la fin de la zone de dépassement autorisé</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="ZONES_EXCLUSION">
        <xsd:annotation>
          <xsd:documentation>Ensemble des zones dans lesquelles le trafic ne doit pas être pris en compte. A exploiter au niveau des outils de post-traitement.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="ZONE_EXCLUSION">
              <xsd:annotation>
                <xsd:documentation>Zone dans laquelle le trafic ne doit pas être pris en compte. A exploiter au niveau des outils de post-traitement.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute default="0" name="position_debut" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Position curviligne du début de la zone d'exclusion</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="INF" name="position_fin" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Position curviligne de la fin de la zone d'exclusion</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Z_LEVEL_CROSSINGS">
        <xsd:annotation>
          <xsd:documentation>Définition des portions du tronçon pour lesquellse on définit un zLevel particulier</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="1">
            <xsd:element maxOccurs="unbounded" name="Z_LEVEL_CROSSING">
              <xsd:complexType>
                <xsd:attribute default="0" name="start" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Gestion des multi-niveaux des éléments du réseau (ponts, tunnels...): position du début de la section du tronçon d'application du z-level
Par défaut, le z-level est appliqué sur tout le tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="INF" name="end" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Gestion des multi-niveaux des éléments du réseau (ponts, tunnels...): position de fin de la section du tronçon d'application du z-level
Par défaut, le z-level est appliqué sur tout le tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="0" name="zlevel" type="xsd:int">
                  <xsd:annotation>
                    <xsd:documentation>Gestion des multi-niveaux des éléments du réseau (ponts, tunnels...): valeur du z-level</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du tronçon</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="nom_axe" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>Nom de l'axe dont le tronçon fait partie</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="id_eltamont" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'élément amont</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="id_eltaval" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'élément aval</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="extremite_amont" type="typeCoordonnees" use="required">
      <xsd:annotation>
        <xsd:documentation>Coordonnées de l'extrémité amont du tronçon</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="extremite_aval" type="typeCoordonnees" use="required">
      <xsd:annotation>
        <xsd:documentation>Coordonnées de l'extrémité aval du tronçon</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="longueur" type="typeFloatPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Longueur en mètre du tronçon dans le cas d'un tronçon curviligne</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="revetement" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>Nom du revêtement (notion utilisée uniquement lors du calcul acoustique). </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="nb_voie" type="typeIntStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Nombre de voie du tronçon</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="largeur_voie" type="typeFloatStrictPositif" use="required">
      <xsd:annotation>
        <xsd:documentation>Largeur d'une voie (en m)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="vit_reg" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Vitesse réglementaire du tronçon (en m/s) (dans le cas d'un tronçon macroscopique, cette vitesse est unique et définie pour le type de véhicule de base, dans le cas d'un tronçon microscopique, elle sera utilisée pour les types de véhicule dont la vitesse réglementaire n'est pas spécifiée dans la liste VITESSES_REG) </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="vit_cat" type="typeFloatStrictPositif" use="optional">
      <xsd:annotation>
        <xsd:documentation>Vitesse moyenne de la catégorie de vitesse (notion de Navstreet, cf. "6.1.27 Speed Category" dans Navstreet Reference Manual)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="true" name="chgt_voie_droite" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si le changement de voie vers la voie la plus à droite est autorisé (oui par défaut)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="chgtvoie_dstfin" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Distance avant la fin du tronçon pour forcer un changement de voie d'un véhicule si il ne se trouve pas sur la bonne voie pour prendre la prochaine direction (en mètres)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="chgtvoie_dstfin_force" type="xsd:float">
      <xsd:annotation>
        <xsd:documentation>Distance avant la fin du tronçon pour forcer un changement de voie d'un véhicule si il ne se trouve pas sur la bonne voie pour prendre la prochaine direction (en mètre), avec un phi arbitraire (attribut chgtvoie_dstfin_force_phi)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="chgtvoie_dstfin_force_phi" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Valeur du Phi à utiliser pour le calcul du changement de voie dans la zone définie par chgtvoie_dstfin_force</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="id_troncon_oppose" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Identifiant du tronçon opposé associé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="exclusion_types_vehicules" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>Liste des identifiants des types de véhicule pour lesquels le tronçon est interdit</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeEntree">
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'entrée</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeSortie">
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la sortie</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeRepartiteur">
    <xsd:complexContent mixed="false">
      <xsd:extension base="typeConnexionInterne">
        <xsd:attribute name="id" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Identifiant du répartiteur</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="controleur_de_feux" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>Identifiant du contrôleur de feux du répartiteur (il doit être défini dans la liste des contrôleurs de feux)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeConvergent">
    <xsd:complexContent mixed="false">
      <xsd:extension base="typeConnexionInterne">
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="TRONCONS_AMONT">
            <xsd:annotation>
              <xsd:documentation>Liste des tronçons amont du convergent</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="1" maxOccurs="unbounded" name="TRONCON_AMONT">
                  <xsd:annotation>
                    <xsd:documentation>Description d'un tronçon amont du convergent</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon amont</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute default="0" name="priorite" type="xsd:nonNegativeInteger">
                      <xsd:annotation>
                        <xsd:documentation>Niveau de priorité du tronçon (1 priorité la plus forte puis plus le niveau augmente, plus la priorité diminue, 0 correspond à une priorité non définie). Si deux tronçons possèdent le même niveau de priorité, SymuVia applique la règle de priorité à droite</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute default="20" name="pos_cpt" type="typeFloatPositif"
                      use="optional">
                      <xsd:annotation>
                        <xsd:documentation>Position (en mètre) du capteur sur le tronçon amont</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Identifiant du convergent</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="id_TAv" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Identifiant du tronçon aval</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type" default="ponctuel">
          <xsd:annotation>
            <xsd:documentation>Indique le type de convergent à utiliser (ponctuel, insertion, ...)</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:NMTOKEN">
              <xsd:enumeration value="ponctuel">
                <xsd:annotation>
                  <xsd:documentation>convergent ponctuel</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="insertion">
                <xsd:annotation>
                  <xsd:documentation>convergent non pocntuel avec voie(s) d'insertion</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute default="0" name="nb_voies_insertion_droite" type="xsd:int">
          <xsd:annotation>
            <xsd:documentation>Nombre de voies d'insertion à droite</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="0" name="nb_voies_insertion_gauche" type="xsd:int">
          <xsd:annotation>
            <xsd:documentation>Nombre de voies d'insertion à droite</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeGiratoire">
    <xsd:complexContent mixed="false">
      <xsd:extension base="typeConnexionInterne">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="TRONCONS_INTERNES">
            <xsd:annotation>
              <xsd:documentation>Liste des tronçons composant l'anneau du giratoire</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="1" maxOccurs="unbounded" name="TRONCON_INTERNE">
                  <xsd:annotation>
                    <xsd:documentation>Caractéristiques d'un tronçon de l'anneau du giratoire</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element minOccurs="0" maxOccurs="1" name="POINTS_INTERNES"
                        type="typePointsInternes">
                        <xsd:annotation>
                          <xsd:documentation>
							Liste des points internes du tronçon
						</xsd:documentation>
                        </xsd:annotation>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon interne</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="troncon_amont" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon définissant l'extrémité amont</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="troncon_aval" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon définissant l'extrémité aval</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute default="0" name="longueur" type="typeFloatPositif"
                      use="optional">
                      <xsd:annotation>
                        <xsd:documentation>Longueur en mètre du tronçon interne dans le cas d'un tronçon curviligne</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="extremite_amont" type="typeCoordonnees" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Coordonnées de l'extrémité amont du tronçon interne</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="extremite_aval" type="typeCoordonnees" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Coordonnées de l'extrémité aval du tronçon interne</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="COEFFICIENTS_INSERTION">
            <xsd:annotation>
              <xsd:documentation>Liste des coefficients permettant de définir manuellement les taux de choix de la voie d'insertion d'un véhicule sur le giratoire (utile pour le cas de giratoires à plusieurs voies) en fonction de la voie d'entrée et du tronçon de sortie</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" name="COEFFICIENT_INSERTION">
                  <xsd:annotation>
                    <xsd:documentation>Définition manuelle des taux de choix de la voie d'insertion d'un véhicule sur le giratoire (utile pour le cas de giratoires à plusieurs voies) pour une voie d'entrée et un troncon de sortie donnés</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:attribute name="troncon_entree" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon d'entrée</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="num_voie_entree" type="xsd:positiveInteger" use="optional"
                      default="1">
                      <xsd:annotation>
                        <xsd:documentation>Numéro de la voie d'entrée</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="troncon_sortie" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon de sortie</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="coeffs" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Liste des coefficients pour chaque voie interne du giratoire (de la voie exterieure vers la voie intérieure)</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Identifiant unique du giratoire</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="troncons" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Liste des tronçons amont et aval du giratoire (ils doivent être décrit dans l'ordre de parcours de l'anneau du giratoire).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="revetement" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>Nom du revêtement à l'intérieur du giratoire</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="vit_max" type="typeFloatStrictPositif" use="optional">
          <xsd:annotation>
            <xsd:documentation>Vitesse maximale sur l'anneau du giratoire (en m/s)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="1" name="nb_voie" type="typeIntStrictPositif">
          <xsd:annotation>
            <xsd:documentation>Nombre de voie sur l'anneau</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LargeurVoie" type="typeFloatStrictPositif" use="required">
          <xsd:annotation>
            <xsd:documentation>Largeur de la voie (en mètre)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="U" name="type">
          <xsd:annotation>
            <xsd:documentation>Type de tronçon</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:NMTOKEN">
              <xsd:enumeration value="D">
                <xsd:annotation>
                  <xsd:documentation>urbain dense</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="U">
                <xsd:annotation>
                  <xsd:documentation>urbain</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
              <xsd:enumeration value="P">
                <xsd:annotation>
                  <xsd:documentation>péri-urbain</xsd:documentation>
                </xsd:annotation>
              </xsd:enumeration>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeCarrefourAFeux">
    <xsd:complexContent mixed="false">
      <xsd:extension base="typeConnexionInterne">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="ENTREES_CAF">
            <xsd:annotation>
              <xsd:documentation>Liste des entrées et de ses caractéristiques du carrefour à feu</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="1" maxOccurs="unbounded" name="ENTREE_CAF">
                  <xsd:annotation>
                    <xsd:documentation>Description d'une entrée du carrefour à feu (une entrée est définie par un tronçon et un numéro de voie)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element minOccurs="0" maxOccurs="1" name="DIVERGENTS">
                        <xsd:annotation>
                          <xsd:documentation>Liste des divergents de l'entrée considérée. Ils sont ordonnés (du 1er au dernier rencontré). Si cette liste n'existe pas, l'ensemble des mouvements issus de l'entrée divergent immédiatement à l'extrémité aval du tronçon d'entrée.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element minOccurs="1" maxOccurs="unbounded" name="DIVERGENT">
                              <xsd:annotation>
                                <xsd:documentation>Description d'un divergent d'une branche d'entrée</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                <xsd:sequence>
                                  <xsd:element minOccurs="0" name="POINTS_INTERNES"
                                    type="typePointsInternes">
                                    <xsd:annotation>
                                      <xsd:documentation>Liste des points internes se situant entre le précédent divergent défini et celui-ci (si c'est le premier divergent défini, c'est entre l'extrémité aval du tronçon d'entrée et celui-ci)</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="id" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du divergent (celui-ci va permettre pour chaque mouvement d'identifier son point de divergence sur la branche principale de l'entrée)</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                      <xsd:maxLength value="128"/>
                                    </xsd:restriction>
                                  </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="coord" type="typeCoordonnees" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Coordonnées du point de divergence</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="MOUVEMENTS">
                        <xsd:annotation>
                          <xsd:documentation>Liste des mouvements possibles issus de l'entrée considéré</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element minOccurs="1" maxOccurs="unbounded" name="MOUVEMENT">
                              <xsd:annotation>
                                <xsd:documentation>Description d'un mouvement</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                <xsd:sequence>
                                  <xsd:element minOccurs="0" name="POINTS_INTERNES"
                                    type="typePointsInternes">
                                    <xsd:annotation>
                                      <xsd:documentation>Liste des points internes du mouvement situés entre le divergent et le convergent de rattachement à la sortie (si aucun point n'est défini, il se situe à l'extrémité aval du tronçon d'entrée du mouvement)</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:element>
                                  <xsd:element minOccurs="0" maxOccurs="1" name="POINTS_D_ATTENTE">
                                    <xsd:annotation>
                                      <xsd:documentation>Liste des points d'attente des traversées du mouvement</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                      <xsd:sequence>
                                        <xsd:element minOccurs="1" maxOccurs="unbounded"
                                          name="POINT_D_ATTENTE">
                                          <xsd:annotation>
                                            <xsd:documentation>Description d'un point d'attente du mouvement. Lors du calcul des points d'intersection des mouvements pour détecter les traversées non prioritaires, celles-ci seront regroupées en considérant leur position par rapport au point d'attente. Ainsi, un point d'attente est utilisé pour les traversées du mouvement se situant en amont du point jusqu'au prochain point d'attente défini ou jusqu'à l'extrémité du mouvement</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:complexType>
                                            <xsd:attribute name="position"
                                              type="typeFloatStrictPositif" use="required">
                                              <xsd:annotation>
                                                <xsd:documentation>Position (abscisse curviligne sur le mouvement) du point d'attente</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute default="0" name="nb_veh_max">
                                              <xsd:annotation>
                                                <xsd:documentation>Nombre maximal de véhicule en attente pouvant s'accumuler au point d'attente
(si 0, pas de nombre maximal)</xsd:documentation>
                                              </xsd:annotation>
                                              <xsd:simpleType>
                                                <xsd:restriction base="xsd:integer">
                                                  <xsd:minInclusive value="0"/>
                                                </xsd:restriction>
                                              </xsd:simpleType>
                                            </xsd:attribute>
                                          </xsd:complexType>
                                        </xsd:element>
                                      </xsd:sequence>
                                    </xsd:complexType>
                                  </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="libelle" use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation>Libellé du mouvement</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                      <xsd:maxLength value="512"/>
                                    </xsd:restriction>
                                  </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="id_troncon_aval" type="xsd:string"
                                  use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du tronçon aval du mouvement</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute default="1" name="num_voie_aval"
                                  type="typeFloatStrictPositif" use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation>Numéro de voie de la sortie du mouvement considéré</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="divergent" type="xsd:string" use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du divergent du mouvement (si il n'existe pas le mouvement est rattaché à l'extrémité aval du tronçon d'entrée)
</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="convergent" type="xsd:string" use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du convergent du mouvement vers la sortie (si il n'existe pas le mouvement est rattaché à l'extrémité amont du tronçon de sortie)</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id_troncon_amont" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon amont de l'entrée considérée</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute default="1" name="num_voie_amont" type="typeFloatStrictPositif"
                      use="optional">
                      <xsd:annotation>
                        <xsd:documentation>Numéro de voie de l'entrée considérée</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute default="defaut" name="priorite" use="optional">
                      <xsd:annotation>
                        <xsd:documentation>Type de priorité de l'entrée du carrefour</xsd:documentation>
                      </xsd:annotation>
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:NMTOKEN">
                          <xsd:enumeration value="defaut">
                            <xsd:annotation>
                              <xsd:documentation>Priorité à droite</xsd:documentation>
                            </xsd:annotation>
                          </xsd:enumeration>
                          <xsd:enumeration value="cedez_le_passage">
                            <xsd:annotation>
                              <xsd:documentation>Cédez-le passage</xsd:documentation>
                            </xsd:annotation>
                          </xsd:enumeration>
                          <xsd:enumeration value="stop">
                            <xsd:annotation>
                              <xsd:documentation>Stop</xsd:documentation>
                            </xsd:annotation>
                          </xsd:enumeration>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="SORTIES_CAF">
            <xsd:annotation>
              <xsd:documentation>Liste des sorties du CAF dans le cas où les convergents de rattachement au sortie ont besoin d'être précisés (sinon tous les mouvements se rattachent a l'extrémité aval de la sortie)</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="1" maxOccurs="unbounded" name="SORTIE_CAF">
                  <xsd:annotation>
                    <xsd:documentation>Description d'une sortie du carrefour à feu (une sortie est définie par un tronçon et un numéro de voie)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="CONVERGENTS">
                        <xsd:annotation>
                          <xsd:documentation>Liste des convergents de la sortie. Ils sont ordonnés à partir de la fin (du dernier au 1er rencontré). Si cette liste n'existe pas, l'ensemble des mouvements arrivant sur cette sortie convergent  immédiatement à l'extrémité amont du tronçon de sortie.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element minOccurs="1" maxOccurs="unbounded" name="CONVERGENT">
                              <xsd:annotation>
                                <xsd:documentation>Description d'un divergent d'un tronçon d'entrée</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                <xsd:sequence>
                                  <xsd:element minOccurs="0" name="POINTS_INTERNES"
                                    type="typePointsInternes">
                                    <xsd:annotation>
                                      <xsd:documentation>Liste des points internes se situant entre le précédent convergent défini et celui-ci (si c'est le premier convergent défini, c'est entre l'extrémité amont du tronçon de sortie et celui-ci)</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="id" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du convergent (celui-ci va permettre pour chaque mouvement d'identifier son point de divergence sur la branche principale de l'entrée)</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                      <xsd:maxLength value="128"/>
                                    </xsd:restriction>
                                  </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="coord" type="typeCoordonnees" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Coordonnées du point de convergence</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id_troncon_aval" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon aval de la sortie considérée</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute default="1" name="num_voie_aval" type="typeFloatStrictPositif"
                      use="optional">
                      <xsd:annotation>
                        <xsd:documentation>Numéro de voie de la sortie considérée</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="id" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>Identifiant unique du carrefour à feu</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="vit_max" type="typeFloatStrictPositif" use="optional">
          <xsd:annotation>
            <xsd:documentation>Vitesse maximale à l'intérieur du carrefour (en m/s)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="controleur_de_feux" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation>Identifiant du contrôleur de feux du carrefour (il doit être défini dans la liste des contrôleurs de feux)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeLTG">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PARAMETRAGE_TYPES_VEHICULES">
        <xsd:annotation>
          <xsd:documentation>Paramétrage des types de véhicules pour l'usage de la flotte parente</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="PARAMETRAGE_TYPE_VEHICULE">
              <xsd:annotation>
                <xsd:documentation>Paramètrage d'un type de véhicule pour l'usage de la flotte parente</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="PARAMETRAGE_CHARGEMENT" type="typeParametrageChargement">
                    <xsd:annotation>
                      <xsd:documentation>Paramètrage lié au chargement / déchargement d'un véhicule</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du type de véhicule</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="LIGNES_TRANSPORT_GUIDEES">
        <xsd:annotation>
          <xsd:documentation>Liste des lignes de transport guidée du réseau</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="LIGNE_TRANSPORT_GUIDEE">
              <xsd:annotation>
                <xsd:documentation>Description d'une ligne de transport guidée</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" name="ORIGINE">
                    <xsd:annotation>
                      <xsd:documentation>Définition de l'origine des bus (si on veut que les bus ne soient pas créés au premier arrêt de la ligne)</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:attribute name="id" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant de l'entrée par laquelle doivent arriver les bus dans le cas où ils ne doivent pas être créés au premier arrêt de la ligne</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="DESTINATION">
                    <xsd:annotation>
                      <xsd:documentation>Définition de la destination des bus (si on veut que les bus ne soient pas détruits au dernier arrêt de la ligne)</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:attribute name="id" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant de la sortie par laquelle doivent sortir les bus dans le cas où ils ne doivent pas être détruits au dernier arrêt de la ligne</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TRONCONS">
                    <xsd:annotation>
                      <xsd:documentation>Itinéraires de la ligne guidée (liste de tronçons successifs de la ligne)
Le VGP est généré au niveau du premier arrêt de cette ligne (le premier arrêt doit donc se trouver sur le premier tronçon listé), sauf si le noeud ORIGINE de la ligne spécifie l'entrée à utiliser (cette origine doit alors être la connexion amont du premier tronçon de la liste).
Le VGP disparaît du réseau après avoir atteint le dernier arrêt de la ligne, ou par la sortie correspondant au noeud DESTINATION de la ligne (cette sortie doit alors être la connexion aval du dernier tronçon de la liste).</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="TRONCON">
                          <xsd:annotation>
                            <xsd:documentation>Tronçon constituant la ligne de transport guidé</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:attribute name="id" type="xsd:string" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Identifiant du tronçon</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="REP_TYPEVEHICULES" type="typeRepTypeVehicules">
                    <xsd:annotation>
                      <xsd:documentation>Description des variantes temporelles de la répartition du flux par type de véhicule (cette répartition est utilisée pour calculer le type de véhicule lors de sa création). Si ce noeud est absent, SymuVia utilise une équirépartition des types de véhicules définis dans les noeuds PARAMETRAGE_TYPE_VEHICULE définis dans le noeud PARAMETRAGE_VEHICULES_GUIDES, ou à défaut le premier type de véhicule défini dans TYPES_DE_VEHICULES.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="JEUX_DE_PARAMETRES">
                    <xsd:annotation>
                      <xsd:documentation>Définition des jeux de paramètres à utiliser en fonction du calendrier.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="PARAMETRES">
                          <xsd:annotation>
                            <xsd:documentation>Définition d'un jeu de paramètres à utiliser en fonction du calendrier</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:attribute name="id" type="xsd:string" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Identifiant du jeu de paramètres</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute default="0" name="dureearrets" use="optional">
                              <xsd:annotation>
                                <xsd:documentation>Liste des durées aux arrêts (en seconde), les durées sont listées dans l'ordre de parcours des arrêts</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                <xsd:restriction>
                                  <xsd:simpleType>
                                    <xsd:list itemType="typeFloatPositif"/>
                                  </xsd:simpleType>
                                </xsd:restriction>
                              </xsd:simpleType>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="CALENDRIER" type="typeCalendrier">
                    <xsd:annotation>
                      <xsd:documentation>Définition des horaires ou fréquences de départ des véhicules de transport guidé</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de la ligne</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="false" name="duree_arrets_dynamique" type="xsd:boolean">
                  <xsd:annotation>
                    <xsd:documentation>Indique si on utilise les paramètres de montée et de descente des passagers des bus pour calculer dynamiquement la durée des arrêts.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute form="unqualified" name="couleur" type="typeRGBAColor" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Indique le couleur utilisée pour représenter la Ligne de transport guidée
</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
              <xsd:key name="LigneBusParametresKey">
                <xsd:selector xpath="./JEUX_DE_PARAMETRES/PARAMETRES"/>
                <xsd:field xpath="@id"/>
              </xsd:key>
              <xsd:keyref name="LigneBusParametresKeyRef" refer="LigneBusParametresKey">
                <xsd:selector xpath="./CALENDRIER/HORAIRE | ./CALENDRIER/FREQUENCE"/>
                <xsd:field xpath="@id_parametres"/>
              </xsd:keyref>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="ARRETS">
        <xsd:annotation>
          <xsd:documentation>Liste des arrêts de la ligne</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="ARRET">
              <xsd:annotation>
                <xsd:documentation>Description d'un arrêt</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="1" minOccurs="0" name="PARAMETRAGE_MONTEE">
                    <xsd:annotation>
                      <xsd:documentation>Liste des variantes temporelles de définition du profil des demandes pour l'arrêt</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="1" name="LIGNE">
                          <xsd:annotation>
                            <xsd:documentation>Définition des paramètres de montée pour une ligne donnée</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element maxOccurs="unbounded" name="DEMANDE">
                                <xsd:annotation>
                                  <xsd:documentation>Définition d'une variante temporelle de demande pour la ligne considérée</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:attribute default="always" name="duree" type="typeDuree">
                                    <xsd:annotation>
                                      <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en seconde) de la variante de la demande pour l'arrêt</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:attribute>
                                  <xsd:attribute name="demande" type="typeFloatPositif"
                                    use="required">
                                    <xsd:annotation>
                                      <xsd:documentation>Valeur de demande (en piéton / s) pour l'arrêt et la ligne sélectionnée</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:attribute>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                            <xsd:attribute name="ligne" type="xsd:string" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Identifiant de la ligne concernée</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute default="0" name="stock_initial"
                              type="xsd:nonNegativeInteger">
                              <xsd:annotation>
                                <xsd:documentation>Stock initial des piétons à l'arrêt pour la ligne considérée</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="typeCreation" default="deterministe">
                              <xsd:annotation>
                                <xsd:documentation>Permet de choisir entre un processus de création des piétons déterministe ou stochastique</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                <xsd:restriction base="xsd:NMTOKEN">
                                  <xsd:enumeration value="deterministe">
                                    <xsd:annotation>
                                      <xsd:documentation>Le nombre de piétons créés pendant une plage temporelle est calculé comme le produit de la demande par la durée de la plage temporelle</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="distributionPoisson">
                                    <xsd:annotation>
                                      <xsd:documentation>Le nombre de piétons créés pendant une plage temporelle est tiré à l'aide d'une distribution de poisson</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                </xsd:restriction>
                              </xsd:simpleType>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element maxOccurs="1" minOccurs="0" name="PARAMETRAGE_DESCENTE">
                    <xsd:annotation>
                      <xsd:documentation>Définition des paramètres de descente des bus pour l'arrêt considéré</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="LIGNE">
                          <xsd:annotation>
                            <xsd:documentation>Définition des paramètres de descente à l'arrêt et pour la ligne considérée</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:attribute name="ligne" type="xsd:string" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Identifiant de la ligne concernée</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="ratio_descente" type="typeFloat01">
                              <xsd:annotation>
                                <xsd:documentation>Proportion des véhicules présents dans le bus à l'approche de l'arrêt qui descendent à cet arrêt. Si ce paramètre n'est pas renseigné, ce ratio prend la valeur 1 (tous les voyageurs descendent à l'arrêt)</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="typeDescente" default="deterministe">
                              <xsd:annotation>
                                <xsd:documentation>Permet de choisir entre un processus de descente des passagers déterministe ou stochastique</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                <xsd:restriction base="xsd:NMTOKEN">
                                  <xsd:enumeration value="deterministe">
                                    <xsd:annotation>
                                      <xsd:documentation>Le nombre de passagers qui descend est calculé comme le produit du ratio_descente et du nombre total de passagers du bus</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                  <xsd:enumeration value="binomial">
                                    <xsd:annotation>
                                      <xsd:documentation>Le nombre de passagers qui descend est tiré à l'aide d'une loi binomiale utilisant le ratio_descente et le nombre total de passagers</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:enumeration>
                                </xsd:restriction>
                              </xsd:simpleType>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de l'arrêt</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="troncon" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Tronçon sur lequel est positionné l'arrêt</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="position" type="typeFloatPositif" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Position de l'arrêt sur le tronçon (en m ou en % de la longueur du tronçon si position_relative vaut true)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="position_relative" type="xsd:boolean" use="optional"
                  default="false">
                  <xsd:annotation>
                    <xsd:documentation>Indique que la position de l'arrêt est exprimée en pourcentage de la longueur du tronçon sur lequel est positionné l'arrêt.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="dureearret" type="xsd:nonNegativeInteger">
                  <xsd:annotation>
                    <xsd:documentation>Durée de l'arrêt (en s) dans le cas d'un paramétrage de type global</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="true" name="pleinevoie" type="xsd:boolean" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Indique si le véhicule guidé s'arrête sur la voie ou en dehors </xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="lignes" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Liste des lignes de transport guidée s'arrêtant à cet arrêt</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction>
                      <xsd:simpleType>
                        <xsd:list itemType="xsd:string"/>
                      </xsd:simpleType>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="num_voies" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Liste des numéros de voie sur lesquelles se trouve l'arrêt pour chacune des lignes concernées par l'arrêt. Le nombre de numéros de voie doit être égal au nombre de lignes passant par cet arrêt et définies dans l'attribut "lignes"</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction>
                      <xsd:simpleType>
                        <xsd:list itemType="xsd:nonNegativeInteger"/>
                      </xsd:simpleType>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute default="true" name="eligible_parc_relais" type="xsd:boolean">
                  <xsd:annotation>
                    <xsd:documentation>Permet d'empêcher si mis à false la liaison entre cet arrêt et les parcs relais proches.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeParametrageCapteurs">
    <xsd:sequence>
      <xsd:element name="CAPTEURS">
        <xsd:annotation>
          <xsd:documentation>Liste des capteurs du réseau</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="CAPTEUR" minOccurs="0"
              type="typeCapteurPonctuel"> </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CAPTEUR_LONGITUDINAL"
              type="typeCapteurLongitudinal"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CAPTEUR_EDIE"
              type="typeCapteurLongitudinal"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CAPTEUR_MFD"
              type="typeCapteurMFD"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CAPTEUR_BLUETOOTH"
              type="typeCapteurBlueTooth"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CAPTEUR_RESERVOIR"
              type="typeCapteurReservoir"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="periodeagregation" type="typeFloatStrictPositif" use="required">
      <xsd:annotation>
        <xsd:documentation>Période d'agrégation (en s) pour les calculs</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="t0" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>instant (en s) à partir duquel démarrer la première période d'acquisition des capteurs</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="periodeagregationEdie" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période d'agrégation à utiliser pour les capteurs d'Edie si différente de la période d'agrégation "periodeagregation"</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="t0Edie" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>instant (en s) à partir duquel démarrer la première période d'acquisition des capteurs d'Edie, si différent du t0 global</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="periodeagregationLongitudinale" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période d'agrégation à utiliser pour les capteurs longitudinaux si différente de la période d'agrégation "periodeagregation"</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="t0Longitudinal" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>instant (en s) à partir duquel démarrer la première période d'acquisition des capteurs longitudinaux, si différent du t0 global</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="periodeagregationMFD" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période d'agrégation à utiliser pour les capteurs MFD si différente de la période d'agrégation "periodeagregation"</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="t0MFD" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>instant (en s) à partir duquel démarrer la première période d'acquisition des capteurs MFD, si différent du t0 global</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="periodeagregationBlueTooth" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période d'agrégation à utiliser pour les capteurs BlueTooth si différente de la période d'agrégation "periodeagregation"</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="t0BlueTooth" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>instant (en s) à partir duquel démarrer la première période d'acquisition des capteurs BlueTooth, si différent du t0 global</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="0" name="t0Reservoir" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>instant (en s) à partir duquel démarrer l'acquisition des capteurs réservoir, si différent du t0 global</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeControleurDeFeux">
    <xsd:annotation>
      <xsd:documentation>Description du type 'CONTROLEUR_DE_FEUX'</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence minOccurs="1" maxOccurs="1">
      <xsd:element minOccurs="0" maxOccurs="1" name="PLANS_DE_FEUX">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès à la liste des plans de feux</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence minOccurs="1" maxOccurs="1">
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="PLAN_DE_FEUX">
              <xsd:annotation>
                <xsd:documentation>Noeud de description d'un plan de feux</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="0" maxOccurs="1" name="SEQUENCES">
                    <xsd:annotation>
                      <xsd:documentation>Noeud d'accès à la liste des séquences du plan de feux</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence minOccurs="1">
                        <xsd:element minOccurs="1" maxOccurs="unbounded" name="SEQUENCE">
                          <xsd:annotation>
                            <xsd:documentation>Noeud de description d'une séquence du plan de feux
</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="1" maxOccurs="1" name="SIGNAUX_ACTIFS">
                                <xsd:annotation>
                                  <xsd:documentation>Noeud d'accès à la liste de description des signaux actifs de la séquence
</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element minOccurs="1" maxOccurs="unbounded"
                                      name="SIGNAL_ACTIF">
                                      <xsd:complexType>
                                        <xsd:attribute name="duree_retard_allumage" use="required">
                                          <xsd:annotation>
                                            <xsd:documentation>Durée en seconde du retard à l'allumage  (cette durée doit être telle que la différence entre la durée de la séquence et la somme du temps de vert et du retard à l'allumage doit être supérieure à la durée de rouge de dégagement définie au niveau du controleur de feux).
</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                            <xsd:restriction base="xsd:int">
                                              <xsd:minInclusive value="0"/>
                                            </xsd:restriction>
                                          </xsd:simpleType>
                                        </xsd:attribute>
                                        <xsd:attribute name="duree_vert" use="required">
                                          <xsd:annotation>
                                            <xsd:documentation>Durée en seconde du temps de vert (cette durée doit être supérieure au temps de vert minimale défini au niveau du contrôleur de feux).
</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                            <xsd:restriction base="xsd:int">
                                              <xsd:minInclusive value="0"/>
                                            </xsd:restriction>
                                          </xsd:simpleType>
                                        </xsd:attribute>
                                        <xsd:attribute name="duree_orange" use="optional"
                                          default="0">
                                          <xsd:annotation>
                                            <xsd:documentation>Durée en seconde du temps d'orange(cette durée peut être nulle).
</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                            <xsd:restriction base="xsd:int">
                                              <xsd:minInclusive value="0"/>
                                            </xsd:restriction>
                                          </xsd:simpleType>
                                        </xsd:attribute>
                                        <xsd:attribute name="troncon_entree" type="xsd:string"
                                          use="required">
                                          <xsd:annotation>
                                            <xsd:documentation>Identifiant unique du tronçon d'entrée du signal
</xsd:documentation>
                                          </xsd:annotation>
                                        </xsd:attribute>
                                        <xsd:attribute name="troncon_sortie" type="xsd:string"
                                          use="required">
                                          <xsd:annotation>
                                            <xsd:documentation>Identifiant unique du tronçon d'entrée du signal
</xsd:documentation>
                                          </xsd:annotation>
                                        </xsd:attribute>
                                      </xsd:complexType>
                                    </xsd:element>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                            <xsd:attribute name="duree_totale" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Durée totale de la séquence en seconde</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                <xsd:restriction base="xsd:int">
                                  <xsd:minExclusive value="0"/>
                                </xsd:restriction>
                              </xsd:simpleType>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="id" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant unique du plan de feux (il permet notamment de lancer son déroulement dans le cas d'un pilotage ou d'identifier le plan de feux utilisé lors de la régulation)</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                      <xsd:maxLength value="80"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="debut" type="xsd:time" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Heure exacte du début du plan de feux à préciser uniquement pour un contrôleur de feux non régulé et si plusieurs plans de feux sont définis</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="LIGNES_GUIDEES_PRIORITAIRES">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès à la liste des lignes guidées prioritaires gérées par le contrôleur de feux parent
</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="LIGNE_GUIDEE_PRIORITAIRE">
              <xsd:annotation>
                <xsd:documentation/>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="lignes" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Liste des lignes de transport guidées circulant sur cette ligne prioritaire</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction>
                      <xsd:simpleType>
                        <xsd:list itemType="xsd:string"/>
                      </xsd:simpleType>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
                <xsd:attribute name="troncon_capteur_amont" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon sur lequel se trouve le capteur amont de la ligne prioritaire (ce capteur permet de détecter l'arrivée d'un véhicule prioritaire au contrôleur de feux)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="position_capteur_amont" type="typeFloatPositif" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Position à laquelle se trouve le capteur amont de la ligne prioritaire (ce capteur permet de détecter l'arrivée d'un véhicule prioritaire au contrôleur de feux)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="troncon_capteur_aval" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon sur lequel se trouve le capteur aval de la ligne prioritaire (ce capteur permet de détecter la sortie du véhicule prioritaire du contrôleur de feux)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="position_capteur_aval" type="typeFloatPositif" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Position à laquelle se trouve le capteur amont de la ligne prioritaire (ce capteur permet de détecter la sortie du véhicule prioritaire du contrôleur de feux)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="troncon_entree_zone" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon permettant de définir l'entrée de la zone du contrôleur de feux</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="position_entree_zone" type="typeFloatPositif" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Position à laquelle se trouve le capteur amont de la ligne prioritaire (ce capteur permet de détecter la sortie du véhicule prioritaire du contrôleur de feux)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="true" name="priorite_totale" type="xsd:boolean">
                  <xsd:annotation>
                    <xsd:documentation>Indique si la priorité des véhicules guidés d cette ligne est totale (ou partagée)</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute default="1" name="sequence_partagee">
                  <xsd:annotation>
                    <xsd:documentation>Numéro d'odre de la séquence partagée dans le cas d'une ligne guidée prioritaire partagée
</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                      <xsd:minInclusive value="1"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="POSITION_FEUX">
        <xsd:annotation>
          <xsd:documentation>Noeud spécifique à l'outils de visualisation Symuplayer permettant de paramétrer la position des feux du contrôleur</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence minOccurs="1" maxOccurs="1">
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="POSITION_FEU">
              <xsd:annotation>
                <xsd:documentation>Description de la position d'un feu pour une entrée du contrôleur</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="troncon_entree" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon d'entrée du feu</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="coord" type="typeCoordonnees" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Coordonnées du feu</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant unique du contrôleur de feu</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:maxLength value="80"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="6" name="duree_vert_min">
      <xsd:annotation>
        <xsd:documentation>Durée (en seconde) minimale de vert du contrôleur de feu. Symubruit vérifie que pour chaque signal actif des séquences des plans de feux du contrôleur de feux, la durée de vert est supérieure à la durée minimale du vert du contrôleur (sinon avertissement).
</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:int">
          <xsd:minExclusive value="0"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="5" name="duree_rouge_degagement">
      <xsd:annotation>
        <xsd:documentation>Durée en seconde du rouge de dégagement. Symubruit vérifie que pour chaque signa actif des séquences des plans de feux du contrôleur de feux, la durée de rouge restante (durée de la séquence - durée du vert du signal - retard à l'allumage du signal) est supérieure à la durée du rouge de dégagement (sinon avertissement).
</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:int">
          <xsd:minExclusive value="0"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="aucune" name="regulation">
      <xsd:annotation>
        <xsd:documentation>Type de régulation du contrôleur de feux :
- aucune
- méthode des vecteurs</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="aucune">
            <xsd:annotation>
              <xsd:documentation>Pas de régulation mise en place</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="vecteurs">
            <xsd:annotation>
              <xsd:documentation>Régulation utilisant la méthode des vecteurs</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute default="180" name="regulation_periode" type="typeIntStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Période de régulation d'un contrôleur de feux en seconde. Cet attribut est utilisé uniquement dans le cas d'un contrôleur de feux régulé. Au début de chaque période, le plan de feux optimal est calculé puis appliqué durant toute la période de régulation suivant.
</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeCapteur">
    <xsd:annotation>
      <xsd:documentation>type de base pour les capteurs</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="troncon" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Tronçon sur lequel est positionné le capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeCapteurPonctuel">
    <xsd:annotation>
      <xsd:documentation>Capteur de type ponctuel (défini par une position ponctuelle sur le tronçon)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="typeCapteur">
        <xsd:attribute name="position" type="typeFloatPositif" use="required">
          <xsd:annotation>
            <xsd:documentation>Position du capteur sur le tronçon (en m ou en % si position_relative vaut true)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="position_relative" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>Indique si la position du capteur est exprimée en pourcentage de la longueur du tronçon plutôt qu'en mètres</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeCapteurLongitudinal">
    <xsd:annotation>
      <xsd:documentation>Capteur de type longitudinal (défini par une position de début et de fin sur le tronçon)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="typeCapteur">
        <xsd:attribute name="position_debut" type="typeFloatPositif" use="required">
          <xsd:annotation>
            <xsd:documentation>Position du début du capteur sur le tronçon (en m ou en % si position_relative vaut true)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="position_fin" type="typeFloatPositif" use="optional" default="INF">
          <xsd:annotation>
            <xsd:documentation>Position de la fin du capteur sur le tronçon (en m ou en % si position_relative vaut true). par défaut, la fin du capteur est la fin du tronçon.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="position_relative" type="xsd:boolean" use="optional">
          <xsd:annotation>
            <xsd:documentation>Indique si la position du capteur est exprimée en pourcentage de la longueur du tronçon plutôt qu'en mètres</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeCapteurMFD">
    <xsd:annotation>
      <xsd:documentation>Capteur d'Edie (possède la particularité de pouvoir s'étendre sur plusieurs tronçons)</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="1" name="TRONCONS">
        <xsd:annotation>
          <xsd:documentation>Liste des tronçons constituant la zone du capteur MFD</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="TRONCON">
              <xsd:annotation>
                <xsd:documentation>Troncon appartenant à la zone MFD</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeCapteurBlueTooth">
    <xsd:annotation>
      <xsd:documentation>Capteur associé à un carrefour à feux ou un répartiteur et qui permet de mesurer les coefficients directionnels de ce noeud</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="POSITION">
        <xsd:annotation>
          <xsd:documentation>Permet de spécifier la positione des capteurs ponctuels détectant les véhicules entrants et sortants du noeud (Par défaut, ces capteurs sont situés aux extrémités des tronçons amont et aval, au niveau du noeud)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="troncon" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Tronçon sur lequel est positionné le capteur</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="position" type="typeFloatPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Position du capteur sur le tronçon (en m ou en % si position_relative vaut true)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="position_relative" type="xsd:boolean" use="optional">
            <xsd:annotation>
              <xsd:documentation>Indique si la position du capteur est exprimée en pourcentage de la longueur du tronçon plutôt qu'en mètres</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="connexion" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du CAF ou répartiteur associé au capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeCapteurReservoir">
    <xsd:annotation>
      <xsd:documentation>Capteur réservoir, associé à une zone et ressortant les instants d'entrée/sortie et la distance parcourue par les véhicules dans la zone. Ignore la période d'agrégation des capteurs.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="1" name="TRONCONS">
        <xsd:annotation>
          <xsd:documentation>Liste des tronçons constituant la zone du capteur réservoir</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="TRONCON">
              <xsd:annotation>
                <xsd:documentation>Troncon appartenant à la zone du capteur réservoir</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType abstract="true" name="typeTraficOrigine">
    <xsd:sequence minOccurs="1">
      <xsd:element name="FLUX_GLOBAL" type="typeFLUX_GLOBAL" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès à la description d'un flux global ayant pour origine l'extrémité courante (pour tous les types de véhicule)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FLUX_TYPEVEHS" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès à la collection des flux par type de véhicule pour l'extrémité courante</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="unbounded">
            <xsd:element name="FLUX_TYPEVEH">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="FLUX" type="typeFLUX">
                    <xsd:annotation>
                      <xsd:documentation>Description du flux pour un type de véhicule pour l'extrémité courante</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="id_typevehicule" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de type de véhicule associé à la description du flux</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="1" name="AGRESSIVITES">
        <xsd:annotation>
          <xsd:documentation>Description du caractère agressif des véhicules issus de l'entrée</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence minOccurs="1" maxOccurs="unbounded">
            <xsd:element minOccurs="0" name="AGRESSIVITE">
              <xsd:annotation>
                <xsd:documentation>Description du caractère agressif d'une classe de véhicule pour cette entrée (par défaut, le taux de véhicule agressif d'une classe est 0)
</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="taux" type="typeFloat01" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Taux de véhicule agressif au sein de la classe 
</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="type_veh" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de la classe de véhicule</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'élément</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="demande" name="typeCreationVehicule" use="optional">
      <xsd:annotation>
        <xsd:documentation>Précise le mode de création de véhicules (liste de véhicule avec l'heure d'entrée sur le réseau ou demande)</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="demande">
            <xsd:annotation>
              <xsd:documentation>Les véhicules sont créés à partir d'une demande en véhicule par seconde. Dès que la demande cumulée atteind 1, un véhicule est créé.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="distributionExponentielle">
            <xsd:annotation>
              <xsd:documentation>Les instants de création des véhicules suivent une distribution exponentielle</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="listeVehicules">
            <xsd:annotation>
              <xsd:documentation>Liste de véhicules avec leur heure d'entrée sur le réseau et leur type</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType abstract="true" name="typeTraficOriginePonctuelle">
    <xsd:complexContent>
      <xsd:extension base="typeTraficOrigine">
        <xsd:sequence minOccurs="0">
          <xsd:element minOccurs="0" name="CREATION_VEHICULES">
            <xsd:annotation>
              <xsd:documentation>Liste des véhicules à créer</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="1" maxOccurs="unbounded" name="CREATION_VEHICULE">
                  <xsd:annotation>
                    <xsd:documentation>Définition d'une création</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:attribute name="instant" type="xsd:float">
                      <xsd:annotation>
                        <xsd:documentation>Instant de création du véhicule</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="typeVehicule" type="xsd:string">
                      <xsd:annotation>
                        <xsd:documentation>Type du véhicule à créer</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="destination" type="xsd:string">
                      <xsd:annotation>
                        <xsd:documentation>Destination du véhicule créé du véhicule</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute default="1" name="num_voie" type="typeIntStrictPositif">
                      <xsd:annotation>
                        <xsd:documentation>Numéro de voie du véhicule à créer</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeTraficExtremite">
    <xsd:complexContent>
      <xsd:extension base="typeTraficOriginePonctuelle">
        <xsd:sequence minOccurs="0">
          <xsd:element minOccurs="0" name="CAPACITES">
            <xsd:annotation>
              <xsd:documentation>Liste des variantes de la capacité de la sortie. Si cette liste n'existe pas, la sortie est libre.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="1" maxOccurs="unbounded" name="CAPACITE">
                  <xsd:annotation>
                    <xsd:documentation>Description d'une variante de la capacité</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:attribute default="always" name="duree" type="typeDuree">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant de la plage temporelle associée, ou durée de la variante</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="valeur" type="typeFloatPositif" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Valeur en veh/sec de la capacité de la sortie pour l'ensemble des voies</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeParkingParameters">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="TYPE_VEHICULE_INTERDIT">
        <xsd:annotation>
          <xsd:documentation>Permet la définition de types de véhicules interdits dans le parking</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="type" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Type de véhicule interdit</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="stock_max" default="-1" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>Stock maximum de véhicules proposé par le parking. -1 : pas de limite (par défaut).</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="stock_initial" default="0" type="xsd:nonNegativeInteger">
      <xsd:annotation>
        <xsd:documentation>Stock initial de véhiules dans le parking</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="inter_temps_sortie" default="0" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Temps minimum entre deux sorties de véhicules</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="inter_temps_entree" default="0" type="typeFloatPositif">
      <xsd:annotation>
        <xsd:documentation>Temps minimum entre deux entrées de véhicules</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="false" name="parc_relais" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si le parking peut faire office de parc relai (possibilité de prendre les transports en commun après s'être garé, on de prendre la voiture au niveau de ce parking après un trajet en transports en commun)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="100" name="parc_relais_rayon" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>rayon autour du centre du parking pour définir les arrêts de transports en commun associés au parc relais</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeTraficParking">
    <xsd:complexContent>
      <xsd:extension base="typeTraficOriginePonctuelle">
        <xsd:sequence>
          <xsd:element name="PARAMETRES" type="typeParkingParameters"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeParkingZone">
    <xsd:complexContent>
      <xsd:extension base="typeParkingParameters">
        <xsd:attribute name="id" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation>identifiant du parking de zone</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeTraficZone">
    <xsd:complexContent>
      <xsd:extension base="typeTraficOrigine">
        <xsd:sequence>
          <xsd:element minOccurs="1" name="TRONCONS">
            <xsd:annotation>
              <xsd:documentation>Liste des tronçons définissant la zone</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="TRONCON">
                  <xsd:annotation>
                    <xsd:documentation>Tronçon appartenant à la zone</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du tronçon</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="capacite_stationnement" default="0" type="xsd:double"
                      use="optional">
                      <xsd:annotation>
                        <xsd:documentation>Capacité de stationnement en mètres. (0 par défaut, -1 : capacité illimitée)</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="stationnement_initial" default="0" type="xsd:double">
                      <xsd:annotation>
                        <xsd:documentation>Valeur de stationnement initiale en mètres.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="PLAQUES">
            <xsd:annotation>
              <xsd:documentation>Liste des plaques de la zone</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" name="PLAQUE">
                  <xsd:annotation>
                    <xsd:documentation>Définition d'une plaque de la zone</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="SECTIONS">
                        <xsd:annotation>
                          <xsd:documentation>Liste des sections de tronçon constituant la plaque</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" name="SECTION">
                              <xsd:annotation>
                                <xsd:documentation>Définition d'une section de tronçon</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                <xsd:attribute name="troncon" type="xsd:string" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du troncon supportant la section</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="position_debut" type="typeFloatPositif"
                                  use="optional" default="0">
                                  <xsd:annotation>
                                    <xsd:documentation>Position du début de la section sur le tronçon (en m ou en % si position_relative vaut true). Par défaut, la section débute au début du tronçon.</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="position_fin" type="typeFloatPositif"
                                  use="optional" default="INF">
                                  <xsd:annotation>
                                    <xsd:documentation>Position de la fin de la section sur le tronçon (en m ou en % si position_relative vaut true). par défaut, la fin de la section est la fin du tronçon.</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="position_relative" type="xsd:boolean"
                                  use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation>Indique si la position de la section est exprimée en pourcentage de la longueur du tronçon plutôt qu'en mètres</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element minOccurs="0" name="REP_MOTIFS_PLAQUES" maxOccurs="1">
                        <xsd:annotation>
                          <xsd:documentation>Définition des coefficients de chaque motif pour la plaque</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" name="REP_MOTIF_PLAQUE">
                              <xsd:annotation>
                                <xsd:documentation>Définition d'un coefficient de motif pour la plaque</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                <xsd:attribute name="motif" type="xsd:string" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du motif considéré</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="coeff" type="typeFloat01" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Coefficient de la plaque pour le motif considéré. La somme pour toutes les plaques d'une zone des coefficients d'un motif référencé dans le noeud REP_MOTIFS de la zone doit être égale à 1.</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                        <xsd:unique name="motifPlaqueUnique">
                          <xsd:selector xpath="./REP_MOTIF_PLAQUE"/>
                          <xsd:field xpath="@motif"/>
                        </xsd:unique>
                      </xsd:element>
                      <xsd:element minOccurs="0" name="PARAMETRAGE_STATIONNEMENT"
                        type="typeParametrageStationnement">
                        <xsd:annotation>
                          <xsd:documentation>Paramètrage du stationnement à utiliser pour la plaque à la place des paramètres généraux ou de ceux définis pour la zone</xsd:documentation>
                        </xsd:annotation>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Nom de la plaque</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element maxOccurs="1" minOccurs="0" name="PARKINGS">
            <xsd:annotation>
              <xsd:documentation>Liste des parkings compris dans la zone</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="PARKING" maxOccurs="unbounded" type="typeParkingZone">
                  <xsd:annotation>
                    <xsd:documentation>Parking appartenant à la zone</xsd:documentation>
                  </xsd:annotation>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="CREATION_VEHICULES">
            <xsd:annotation>
              <xsd:documentation>Liste des véhicules à créer</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element minOccurs="1" maxOccurs="unbounded" name="CREATION_VEHICULE">
                  <xsd:annotation>
                    <xsd:documentation>Définition d'une création</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:attribute name="instant" type="xsd:float">
                      <xsd:annotation>
                        <xsd:documentation>Instant de création du véhicule</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="typeVehicule" type="xsd:string">
                      <xsd:annotation>
                        <xsd:documentation>Type du véhicule à créer</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="destination" type="xsd:string">
                      <xsd:annotation>
                        <xsd:documentation>Destination du véhicule créé du véhicule</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="REPARTITIONS_TYPEVEHICULE">
            <xsd:annotation>
              <xsd:documentation>Liste des variantes des répartitions des types de véhicule</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" name="REPARTITION_TYPEVEHICULE">
                  <xsd:annotation>
                    <xsd:documentation>Description d'une variante d'une répartition des types de véhicule</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:attribute default="always" name="duree" type="typeDuree">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en seconde) de la variante de la répartition des types de véhicule</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="valeurs" type="typeFloat01NormedList" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Liste ordonnée (selon la définition des types de véhicule)</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="PARAMETRAGE_STATIONNEMENT"
            type="typeParametrageStationnement">
            <xsd:annotation>
              <xsd:documentation>Paramètrage du stationnement à utiliser pour la zone à la place des paramètres généraux</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="zone_distance_terminaison_max" type="typeFloatStrictPositif">
          <xsd:annotation>
            <xsd:documentation>Radius pour l'orientation des positions tirées en zone en mode distribué (inactif si on utilise des plaques)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeRegulation">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DETECTEURS">
        <xsd:annotation>
          <xsd:documentation>Ensemble des détecteurs associés à la brique de régulation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CAPTEUR">
              <xsd:annotation>
                <xsd:documentation>Capteur associé à la brique de régulation</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:all minOccurs="0">
                  <xsd:element minOccurs="0" name="PARAMETRES" type="typeAny">
                    <xsd:annotation>
                      <xsd:documentation>Définition des paramètres nécessaire à la fonction définie pour le capteur</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:all>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du capteur</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="fonction" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Fonction du capteur</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="module" type="xsd:string" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Chemin du fichier python contenant la fonction, uniquement s'il s'agit d'une fonction non incluse dans les scripts fournis avec symubruit</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="DECLENCHEURS">
        <xsd:annotation>
          <xsd:documentation>Ensemble des déclencheurs associés à la brique de régulation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CONDITION">
              <xsd:annotation>
                <xsd:documentation>Condition de déclenchement d'actions associée à la brique de régulation</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:all minOccurs="0">
                  <xsd:element minOccurs="0" name="PARAMETRES" type="typeAny">
                    <xsd:annotation>
                      <xsd:documentation>Définition des paramètres nécessaire à la fonction définie pour la condition</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:all>
                <xsd:attribute name="id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant de la condition</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="fonction" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Fonction de la condition</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="module" type="xsd:string" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Chemin du fichier python contenant la fonction, uniquement s'il s'agit d'une fonction non incluse dans les scripts fournis avec symubruit</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ACTIONS">
        <xsd:annotation>
          <xsd:documentation>Ensemble des actions associées à la brique de régulation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="ACTION">
              <xsd:annotation>
                <xsd:documentation>Action associée à la brique de régulation</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:all minOccurs="0">
                  <xsd:element minOccurs="0" name="PARAMETRES" type="typeAny">
                    <xsd:annotation>
                      <xsd:documentation>Définition des paramètres nécessaire à la fonction définie pour l'action</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:all>
                <xsd:attribute name="conditions" type="xsd:string" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Liste des identifiants des conditions nécessaires à l'exécution de l'action</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="fonction" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Fonction de l'action</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="module" type="xsd:string" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Chemin du fichier python contenant la fonction, uniquement s'il s'agit d'une fonction non incluse dans les scripts fournis avec symubruit</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="RESTITUTIONS">
        <xsd:annotation>
          <xsd:documentation>Ensemble des définitions de restitution dans le fichier de sortie pour la brique de régulation</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="RESTITUTION">
              <xsd:annotation>
                <xsd:documentation>Définition des sorties à produire dans le fichier de sortie poru la brique de régulation</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:all minOccurs="0">
                  <xsd:element minOccurs="0" name="PARAMETRES" type="typeAny">
                    <xsd:annotation>
                      <xsd:documentation>Définition des paramètres nécessaire à la fonction définie pour la restitution</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:all>
                <xsd:attribute name="fonction" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Fonction de la resitution</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="module" type="xsd:string" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Chemin du fichier python contenant la fonction, uniquement s'il s'agit d'une fonction non incluse dans les scripts fournis avec symubruit</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la brique de régulation</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="taux_respect_consigne" type="typeFloat01">
      <xsd:annotation>
        <xsd:documentation>Taux de respect de la consigne</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeCarFollowing">
    <xsd:sequence>
      <xsd:element name="FONCTIONS">
        <xsd:annotation>
          <xsd:documentation>Définition des fonctions python à utiliser pour les différentes composantes du modèle de loi de poursuite</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="loi" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Fonction principale de la loi de poursuite : calcul de la valeur de la variable d'état.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="distanceInfluenceMax" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Fonction calculant la distance d'influence maximum de la loi de poursuite</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="longueurVehicule" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Fonction renvoyant la longueur du véhicule. SI cette fonction n'est pas définie, la longueur des véhicules sera calculée comme étant 1/Kmax - espaccement à l'arrêt</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="vitesseApprochee" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Dans le cas de l'utilisation de la loi de poursuite par défaut (Newell) pour au moins un véhicule du réseau, cette fonction est nécessaire afin de permettre aux véhicules qui utilisent Newell d'estimer la vitesse d'un leader qui suit la loi de poursuite définie ici sans exécuter son modèle de loi de poursuite complet. Cette fonction n'est appelée que dans le cas d'une "boucle" de véhicules.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="agressivite" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Procédure de gestion de l'agressivité à exécuter dans le cadre de la loi de poursuite.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="testChangementVoie" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Fonction qui teste si le véhicule change de voie ou non. Si cette fonction n'est pas définie, la fonction de changement de voie par défaut (Newell) est utilisée</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="appliqueChangementVoie" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Fonction appelée en cas de changement de voie réussi par un véhicule, si la loi de poursuite prévoit dans ce cas d'effectuer une action particulière (par exemple, dans le cas Newell, cette fonction peut créer un véhicule ghost lié au changement de voie effectué).</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="calculVitesseVoie" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Calcul de la vitesse d'une voie, utilisée dans le cas d'un chamgement de voie si l'attribut chgtvoie_discr_ordre vaut 'F' (voie la plus favorable)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="testInsertionConvergent" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Fonction qui teste si le véhicule peut s'insérer ou non à un convergent. Si la fonction n'est pas définie, la fonction de test d'insertion à un convergent par défauut (Newell) est utilisée.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="module" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation>Chemin du fichier python contenant les fonctions, dans le cas d'utilisation de fonctions non incluses dans les scripts fournis avec symubruit</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du modèle de loi de poursuite</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="variable_etat" default="position">
      <xsd:annotation>
        <xsd:documentation>Variable d'état calculée par la loi de poursuite (position, vitesse ou accélération)</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:NMTOKEN">
          <xsd:enumeration value="position">
            <xsd:annotation>
              <xsd:documentation>La loi de poursuite calcule la variable d'état 'position'. SymuBruit en déduit la vitesse et accélération correspondante</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="vitesse">
            <xsd:annotation>
              <xsd:documentation>La loi de poursuite calcule la variable d'état 'vitesse'. SymuBruit en déduit la position et accélération correspondante</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="acceleration">
            <xsd:annotation>
              <xsd:documentation>La loi de poursuite calcule la variable d'état 'acceleration'. SymuBruit en déduit la position et vitesse correspondante</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeITINERAIRE">
    <xsd:choice minOccurs="1">
      <xsd:element name="TRONCONS_" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Liste des tronçons constituant l'itinéraire</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="TRONCON_">
              <xsd:complexType>
                <xsd:attribute name="libelle" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Identifiant du tronçon</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="NOEUD">
        <xsd:annotation>
          <xsd:documentation>Noeud correspondant à l'itinéraire (cas par exemple d'un itinéraire de zone à zone en contact)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Identifiant du noeud</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
    <xsd:attribute name="coeffAffectation" type="typeFloat01" use="required">
      <xsd:annotation>
        <xsd:documentation>Coefficient d'affectation des vahicules à cet itinéraire. La somme des coefficients des itinéraires doit être égale à 1.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="id" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'itinéraire défini au niveau du noeud RESEAU/ROUTES. Si cet identifiant n'est pas défini, l'utilisateur doit listé les tronçons de l'itinéraire au niveau du noeud enfant TRONCONS_</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeFLUX">
    <xsd:all>
      <xsd:element minOccurs="0" name="DEMANDES">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès aux variantes temporelles de la demande pour l'extrémité courante</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="DEMANDE" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Description d'une variante temporelle de demande de l'extrémité courante</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="niveau" type="typeFloatPositif">
                  <xsd:annotation>
                    <xsd:documentation>Niveau de la demande en veh/s</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="duree" type="typeDuree" default="always" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle
</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="REP_DESTINATIONS" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès aux variantes temporelles de la répartition vers les destinations pour l'extrémité courante</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="REP_DESTINATION" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Description de la variante temporelle de la répartition vers les destinations</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="DESTINATION" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Description d'une destination de l'extrémité courante
</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:annotation>
                          <xsd:documentation>Liste des itinéraires affectés</xsd:documentation>
                        </xsd:annotation>
                        <xsd:element name="ROUTE" type="typeITINERAIRE" maxOccurs="unbounded"
                          minOccurs="0"/>
                      </xsd:sequence>
                      <xsd:attribute name="sortie" type="xsd:string" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant de la sortie destination</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="coeffOD" type="typeFloat01" use="required">
                        <xsd:annotation>
                          <xsd:documentation>Coefficient de la matrice OD pour l'origine (extrélité courante) et la destination considérée</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_teta_logit" type="typeFloatStrictPositif"
                        use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Paramètre teta du modèle logit de l'affectation spécifique pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="nombre_pluscourtchemin" type="typeIntStrictPositif"
                        use="optional">
                        <xsd:annotation>
                          <xsd:documentation>Nombre de plus court chemin à considérer dans le cas d'un comportment de flux de type 'itineraire' pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_commonality_alpha" type="xsd:float">
                        <xsd:annotation>
                          <xsd:documentation>Valeur maximum du commonality factor admissible pour les itinéraires calculés par le module d'affectation pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_commonality_beta" type="typeFloat01">
                        <xsd:annotation>
                          <xsd:documentation>Valeur du paramètre beta pour le calcul du commonality factor pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute name="affectation_commonality_gamma" type="typeFloat01">
                        <xsd:annotation>
                          <xsd:documentation>Valeur du paramètre gamma pour le calcul du commonality factor pour ce couple OD</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="duree" type="typeDuree" default="always" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="REP_VOIES" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Noeud d'accès aux variantes temporelles de la répartition par voie du flux provenant de l'extrémité (origine) courante</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="REP_VOIE" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Description d'une variante temporelle de la répartition par voie du flux provenant de l'extrémité (origine) courante</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="duree" type="typeDuree" default="always">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="coeffs" default="1" type="typeFloat01NormedList">
                  <xsd:annotation>
                    <xsd:documentation>Liste ordonnée des coefficients de répartition sur les voies du tronçon amont de l'extrémité courante</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="REP_MOTIFS">
        <xsd:annotation>
          <xsd:documentation>Liste des variantes temporelles de la distribution des motifs "origine" + "destination" pour les véhicules créés par la demande</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="REP_MOTIF">
              <xsd:annotation>
                <xsd:documentation>Définition d'une variante temporelle de distribution des motifs "origine" + "destination" pour les véhicules créés par la demande</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="REP_MOTIF_DEST">
                    <xsd:annotation>
                      <xsd:documentation>Définition de la distribution des motifs "origine" + "destination" pour une destination donnée (ou pour toutes les destinations si l'attribut "destination" n'est pas renseigné)</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="MOTIF_COEFF">
                          <xsd:annotation>
                            <xsd:documentation>Définition de l'association entre un couple (motif origine, motif destination) et le coefficient associé</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:attribute name="motif_origine" type="xsd:string" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Identifiant du motif d'origine</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="motif_destination" type="xsd:string" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Identifiant du motif de destination</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute name="coeff" type="typeFloat01" use="required">
                              <xsd:annotation>
                                <xsd:documentation>Coefficient du couple motif origine et motif destination pour le flux considéré</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute name="destination" type="xsd:string">
                        <xsd:annotation>
                          <xsd:documentation>Identifiant de la destination concernée pour la répartition des motifs. L'absence de cet attribut indique que la répartition des motifs est identique pour toutes les destinations. </xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="duree" type="typeDuree" default="always" use="optional">
                  <xsd:annotation>
                    <xsd:documentation>Durée de la variante temporelle
</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="typeFLUX_GLOBAL">
    <xsd:sequence>
      <xsd:element name="FLUX" type="typeFLUX">
        <xsd:annotation>
          <xsd:documentation>Description du flux </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="REP_TYPEVEHICULES" type="typeRepTypeVehicules">
        <xsd:annotation>
          <xsd:documentation>Description des variantes temporelles de la répartition du flux par type de véhicule (cette répartition est utilisée pour calculer le type de véhicule lors de sa création)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeRepTypeVehicules">
    <xsd:sequence>
      <xsd:element name="REP_TYPEVEHICULE" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Description d'une variante temporelle de la répartition du flux par type de véhicule</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="duree" type="typeDuree" default="always">
            <xsd:annotation>
              <xsd:documentation>Durée de la variante temporelle</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="coeffs" type="typeFloat01List">
            <xsd:annotation>
              <xsd:documentation>Liste ordonnée des coefficients de répartition du flux selon le type de véhicule. La somme doit être égale à 1, la liste doit contenir auntant d'élément que de type de véhicule défini, l'ordre des coefficients doit être identique à celui de la liste des types de véhicules.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeCalendrier">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" ref="CALENDRIER_ELEMENT"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element abstract="true" name="CALENDRIER_ELEMENT"/>
  <xsd:element name="HORAIRE" substitutionGroup="CALENDRIER_ELEMENT">
    <xsd:annotation>
      <xsd:documentation>Définition d'un départ à un instant donné</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="heuredepart" type="xsd:time" use="required">
        <xsd:annotation>
          <xsd:documentation>Heure de départ</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="id_parametres" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Identifiant éventuel du jeu de paramètres à utiliser pour les départs provoqués par ce noeud.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FREQUENCE" substitutionGroup="CALENDRIER_ELEMENT">
    <xsd:annotation>
      <xsd:documentation>Définition d'une fréquence de départ à respecter pendant une période de temps</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="heuredepart" type="xsd:time" use="required">
        <xsd:annotation>
          <xsd:documentation>Heure de départ du permier départ, et heure de début de respect de la fréquence définie par l'attribut "frequence".</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="heurefin" type="xsd:time" use="required">
        <xsd:annotation>
          <xsd:documentation>Heure de fin du respect de la fréquence définie par le noeud "frequence". Si cette heure de fin correspond à un départ dans le respect de la fréquence, ce départ est effectué.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="frequence" type="typeIntStrictPositif" use="required">
        <xsd:annotation>
          <xsd:documentation>Fréquence en seconde entre le lancement de 2 véhicules (à définir uniquement si le type de paramétrage est global)</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="id_parametres" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Identifiant éventuel du jeu de paramètres à utiliser pour les départs provoqués par ce noeud.</xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType abstract="true" name="typeFlotte">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="REP_TYPEVEHICULES" type="typeRepTypeVehicules">
        <xsd:annotation>
          <xsd:documentation>Description des variantes temporelles de la répartition du flux par type de véhicule (cette répartition est utilisée pour calculer le type de véhicule lors de sa création). Si ce noeud est absent, SymuVia utilise une équirépartition des types de véhicules définis dans les noeuds PARAMETRAGE_TYPE_VEHICULE définis pour la flotte correspondante, ou à défaut le premier type de véhicule défini dans TYPES_DE_VEHICULES.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeFlotteLivraison">
    <xsd:complexContent>
      <xsd:extension base="typeFlotte">
        <xsd:sequence>
          <xsd:element name="PARAMETRAGE_TYPES_VEHICULES">
            <xsd:annotation>
              <xsd:documentation>Paramétrage des types de véhicules pour l'usage de la flotte parente</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" name="PARAMETRAGE_TYPE_VEHICULE">
                  <xsd:annotation>
                    <xsd:documentation>Paramètrage d'un type de véhicule pour l'usage de la flotte parente</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="PARAMETRAGE_CHARGEMENT" type="typeParametrageChargement">
                        <xsd:annotation>
                          <xsd:documentation>Paramètrage lié au chargement / déchargement d'un véhicule</xsd:documentation>
                        </xsd:annotation>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du type de véhicule</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element minOccurs="0" name="CENTRES_LOGISTIQUES">
            <xsd:annotation>
              <xsd:documentation>Liste des centres logistiques correspondants à la flotte des véhicules de livraison</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" name="CENTRE_LOGISTIQUE">
                  <xsd:annotation>
                    <xsd:documentation>Centre logistique correspondant à la flotte des véhicules de livraison</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="POSITION_ENTREE" type="typePositionConnexion">
                        <xsd:annotation>
                          <xsd:documentation>Position à laquelle les véhicules entrent dans le centre logistique.</xsd:documentation>
                        </xsd:annotation>
                      </xsd:element>
                      <xsd:element name="POSITION_SORTIE" type="typePositionConnexion">
                        <xsd:annotation>
                          <xsd:documentation>Position à laquelle les véhicules sortent du centre logistique.</xsd:documentation>
                        </xsd:annotation>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du centre logistique.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="POINTS_DE_LIVRAISON">
            <xsd:annotation>
              <xsd:documentation>Liste des points de livraison</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" name="POINT_DE_LIVRAISON">
                  <xsd:annotation>
                    <xsd:documentation>Point de livraison</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element name="POSITION" type="typePositionTroncon">
                        <xsd:annotation>
                          <xsd:documentation>Position du point de livraison</xsd:documentation>
                        </xsd:annotation>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant du point de livraison.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="nb_places" type="typeIntStrictPositif">
                      <xsd:annotation>
                        <xsd:documentation>Nombre de places disponibles sur l'aire de livraison. Une fois la limite atteinte, les véhicules doivent attendre sur la voie avant de pouvoir se ranger sur l'air de livraison. Par défaut, il n'y a pas de limite de places. Ce paramètre n'a de sens que pour les points de livraison hors voie (si un point de livraison est sur la voie, une fois un premier véhicule à l'arrpet, les véhicules suivants ne peuvent de toute façon pas l'atteindre tant que le premier véhicule n'est pas parti).</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="TOURNEES_DE_LIVRAISON">
            <xsd:annotation>
              <xsd:documentation>Liste des tournées de livraison</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element maxOccurs="unbounded" name="TOURNEE_DE_LIVRAISON">
                  <xsd:annotation>
                    <xsd:documentation>Tournée de livraison</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:sequence>
                      <xsd:element minOccurs="1" name="ORIGINE">
                        <xsd:annotation>
                          <xsd:documentation>Définition de l'origine de la tournée (centre logistique ou extremité)</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:attribute name="id" type="xsd:string" use="required">
                            <xsd:annotation>
                              <xsd:documentation>Identifiant de l'entrée ou du centre logistique par lequel doit démarrer la tournée de livraison</xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element minOccurs="1" name="DESTINATION">
                        <xsd:annotation>
                          <xsd:documentation>Définition de la destination de la tournée (centre logistique ou extremité)</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:attribute name="id" type="xsd:string" use="required">
                            <xsd:annotation>
                              <xsd:documentation>Identifiant de l'a sortie ou du centre logistique par lequel doit se terminer la tournée de livraison</xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="POINTS_DE_LIVRAISON">
                        <xsd:annotation>
                          <xsd:documentation>Liste des points de livraison constituant la tournée à réaliser.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                          <xsd:sequence>
                            <xsd:element maxOccurs="unbounded" name="POINT_DE_LIVRAISON">
                              <xsd:annotation>
                                <xsd:documentation>Point de livraison constituant la tournée à réaliser.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                <xsd:attribute name="id" type="xsd:string" use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Identifiant du point de livraison</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="dechargement" type="xsd:nonNegativeInteger"
                                  use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Quantité à livrer à ce point de livraison</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="chargement" type="xsd:nonNegativeInteger"
                                  use="required">
                                  <xsd:annotation>
                                    <xsd:documentation>Quantité à récupérer à ce point de livraison</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:complexType>
                      </xsd:element>
                      <xsd:element name="CALENDRIER" type="typeCalendrier">
                        <xsd:annotation>
                          <xsd:documentation>Définition des horaires ou fréquences de départ de la tournée de livraison</xsd:documentation>
                        </xsd:annotation>
                      </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="id" type="xsd:string" use="required">
                      <xsd:annotation>
                        <xsd:documentation>Identifiant de la tournée</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="typeParametrageChargement">
    <xsd:attribute default="false" name="montee_descente_simultanee" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indique si le chargement et le déchargement du véhicule sont possible simultanément</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="temps_montee_individuelle" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Temps de montée individuelle dans le véhicule (en secondes par unité de stock). Permet de calculer dynamiquement le temps des arrêts.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="1" name="temps_descente_individuelle" type="typeFloatStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Temps de descente individuelle dans le véhicule (en secondes par unité de stock). Permet de calculer dynamiquement le temps des arrêts.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="charge_max" type="typeIntStrictPositif">
      <xsd:annotation>
        <xsd:documentation>Capacité de chargement maximal du véhicule (pas de maximum si pas défini)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typePositionConnexion">
    <xsd:sequence>
      <xsd:element name="POSITION_CONNEXION">
        <xsd:annotation>
          <xsd:documentation>Position sur une connexion.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Identifiant de la connexion correspondant à la position.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typePositionTroncon">
    <xsd:sequence>
      <xsd:element name="POSITION_TRONCON">
        <xsd:annotation>
          <xsd:documentation>Position le long d'un tronçon.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="id" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation>Identifiant du tronçon correspondant à la position.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="position" type="typeFloatPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Position curviligne sur le tronçon (en m ou en % de la longueur du tronçon si position_relative vaut true)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="position_relative" type="xsd:boolean" use="optional" default="false">
            <xsd:annotation>
              <xsd:documentation>Indique que la position est exprimée en pourcentage de la longueur du tronçon.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="hors_voie" type="xsd:boolean" use="optional" default="false">
            <xsd:annotation>
              <xsd:documentation>Indique si la position se trouve hors voie (ne gêne pas la circulation) ou non.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="num_voie" type="typeIntStrictPositif" use="optional">
            <xsd:annotation>
              <xsd:documentation>Numéro de la voie de la position sur le tronçon. Par défaut, la position n'est pas associée à une voie particulière.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeParametrageStationnement">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="PARAMETRES_STATIONNEMENT">
        <xsd:annotation>
          <xsd:documentation>Variante temporelle de paramètres de stationnement. Peut être spécialisé par zone ou plaque.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute default="always" name="duree" type="typeDuree">
            <xsd:annotation>
              <xsd:documentation>Identifiant de la plage temporelle associée, ou durée (en secondes) de la variante des paramètres de stationnement</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="proportion_stationnement_residentiel_origine" type="typeFloat01"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Pourcentage de véhicules issus d'un stationnement résidentiel (véhicules non consommateurs d'espace de stationnement)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="proportion_stationnement_residentiel_destination" type="typeFloat01"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Pourcentage de véhicules à destination d'un stationnement résidentiel (véhicules non consommateurs d'espace de stationnement)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="proportion_stationnement_surfacique_origine" type="typeFloat01"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Probabilité de choisir pour l'origine d'un véhicule en stationnement non résidentiel un parking surfacique plutôt qu'un parking hors sol.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="proportion_stationnement_surfacique_destination" type="typeFloat01"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Probabilité d'arbitrage vers un stationnement surfacique (plutôt que vers un parking hors sol), sauf si prise_en_compte_couts vaut true.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="prise_en_compte_couts" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Si true, les attributs cout_stationnement_surfacique, cout_stationnement_parking et cout_stationnement_ratio sont utilisés pour le calcul de la probabilité de destination surfacique ou hors sol à la place de la valeur donnée par l'atribut proportion_stationnement_surfacique_destination.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="cout_stationnement_surfacique" type="typeFloatPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Coût associé au stationnement surfacique</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="cout_stationnement_parking" type="typeFloatPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Coût associé au stationnement non surfacique (parkings)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="cout_stationnement_ratio" type="typeFloatPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>facteur multiplicatif à appliquer à cout_stationnement_parking avant de calculer le rapport cout_surfacique / cout_parking pour les tirage entre stationnement surfacique et parking</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="duree_recherche_surfacique_max" type="typeFloatStrictPositif"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Durée maximale de recherche de stationnement surfacique avant destruction du véhicule</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="frequence_rearbitrage" type="typeFloatStrictPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Fréquence de réarbitrage entre stationnement surfacique et hors sol alors que le véhicule est en recherche de stationnement surfacique, en secondes</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="delai_elargissement_recherche" type="typeFloatPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Délai en secondes à partir duquel le véhicule étend sa zone de recherche de stationnement (ignore le rayon de recherche en zone si zone, ou élargi la plaque du rayon de recherche en zone si plaque), à partir du début de la recherche de stationnement.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="generation_demande_stationnement" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Si activé, les véhicules qui entrent en stationnement non résidentiel tirent une durée de stationnement et viennent augmenter la demande de la zone une fois cette durée écoulée.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="duree_stationnement_moyenne" type="typeFloatStrictPositif"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Valeur moyenne de la durée de stationnement (inutilisé si generation_demande_stationnement vaut false), en secondes</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="duree_stationnement_ecart_type" type="typeFloatPositif"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Valeur de l'écart type de la durée de stationnement (inutilisé si generation_demande_stationnement vaut false), en secondes</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="mode_surfacique_simplifie" type="xsd:boolean" use="required">
            <xsd:annotation>
              <xsd:documentation>Si true, la recherche de stationnement surfacique se base sur une distance à parcourir, calculée à l'aide du taux d'occupation surfacique et des paramètres de description du stationnement surfacique distance_entre_blocs, distance_entre_places et nb_places_par_bloc.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="frequence_maj_distance_recherche" type="typeFloatStrictPositif"
            use="required">
            <xsd:annotation>
              <xsd:documentation>Fréquence de mise à jour de la distance à parcourir en mode de recherche de stationnement surfacique simplifié, en secondes</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="distance_entre_blocs" type="typeFloatStrictPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Distance entre deux blocs de stationnement surfacique</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="distance_entre_places" type="typeFloatStrictPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Distance entre deux places de stationnement surfacique</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="nb_places_par_bloc" type="typeFloatStrictPositif" use="required">
            <xsd:annotation>
              <xsd:documentation>Nombre de places de stationnement surfacique par bloc</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typeParametrageStationnementGeneral">
    <xsd:complexContent>
      <xsd:extension base="typeParametrageStationnement">
        <xsd:attribute name="gestion_stationnement" type="xsd:boolean" use="required">
          <xsd:annotation>
            <xsd:documentation>Permet d'activer ou non la gestion du stationnement. Si désactivé, les départs/terminaisons en zones sont toutes considérées comme étant du stationnement résidentiel.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute default="false" name="creation_vehicules_sans_stock" type="xsd:boolean">
          <xsd:annotation>
            <xsd:documentation>Si true, permet de créer les véhicules à l'origine en zone même si le stock disponible est nul (permet de respecter le taux de consigne de demande quoi qu'il arrive).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="nb_places_max_par_parking" type="xsd:positiveInteger" use="required">
          <xsd:annotation>
            <xsd:documentation>Nombre de places de parking à considérer pour les parkings avec capacité infinie pour l'arbitrage entre les différents parkings</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="ROOT_SYMUBRUIT">
    <xsd:annotation>
      <xsd:documentation>Noeud racine des données d'entrée de la plateforme SymuVia</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="PLAGES_TEMPORELLES">
          <xsd:annotation>
            <xsd:documentation>Ensemble des plages temporelles définies pour l'ensemble de la ou des simulations, dont l'ID peut être utilisé pour renseigner la plage de validité des variantes temporelles (attributs "duree" de certains éléments)</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element maxOccurs="unbounded" name="PLAGE_TEMPORELLE" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Plage temporelle pouvant être utilisée en mode "horaire" pour renseigner la plage de validité des variantes temporelles (attributs "duree" de certains éléments)</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:attribute name="id" type="xsd:ID" use="required">
                    <xsd:annotation>
                      <xsd:documentation>identifiant de la plage temporelle</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="debut" type="xsd:time" use="required">
                    <xsd:annotation>
                      <xsd:documentation>date de début de la plage temporelle (hh:mm:ss)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="fin" type="xsd:time" use="required">
                    <xsd:annotation>
                      <xsd:documentation>date de fin de la plage temporelle (hh:mm:ss)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
            <xsd:attribute name="type" default="profil">
              <xsd:annotation>
                <xsd:documentation>Type de définition des variations temporelles ('profil' ou 'horaire')</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:NMTOKEN">
                  <xsd:enumeration value="profil">
                    <xsd:annotation>
                      <xsd:documentation>Définition de variantes temporelles définies par liste de durées successives</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                  <xsd:enumeration value="horaire">
                    <xsd:annotation>
                      <xsd:documentation>Définition de variantes temporelles définies sous forme d'un ensemble de plages (heure de début et heure de fin)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:enumeration>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="debut" type="xsd:time" use="optional">
              <xsd:annotation>
                <xsd:documentation>Heure de démarrage des premières variantes temporelles en mode 'profil'</xsd:documentation>
              </xsd:annotation>
            </xsd:attribute>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="SIMULATIONS">
          <xsd:annotation>
            <xsd:documentation>Liste des définitions de paramètres de simulation</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="SIMULATION" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Paramétrage de la simulation</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:complexContent mixed="false">
                    <xsd:extension base="typeParametrageSimulation"/>
                  </xsd:complexContent>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="TRAFICS">
          <xsd:annotation>
            <xsd:documentation>Liste des définitions de paramètres de trafic</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="TRAFIC" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Paramétrage du trafic</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:complexContent mixed="false">
                    <xsd:extension base="typeParametrageTrafic"/>
                  </xsd:complexContent>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="RESEAUX">
          <xsd:annotation>
            <xsd:documentation>Liste des définitions de réseaux</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="RESEAU" maxOccurs="unbounded">
                <xsd:annotation>
                  <xsd:documentation>Données du réseau</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:all>
                    <xsd:element name="TRONCONS">
                      <xsd:annotation>
                        <xsd:documentation>Liste des tronçons</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" name="TRONCON" type="typeTroncon">
                            <xsd:annotation>
                              <xsd:documentation>Description d'un tronçon</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="CONNEXIONS">
                      <xsd:annotation>
                        <xsd:documentation>Noeud </xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:all>
                          <xsd:element name="EXTREMITES" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Liste des extrémités (anciennement entrées et sorties)</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="EXTREMITE">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'une extremité du réseau</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:complexType>
                                    <xsd:attribute name="id" type="xsd:string" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Identifiant de l'extremité</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element minOccurs="0" name="PARKINGS">
                            <xsd:annotation>
                              <xsd:documentation>Liste des parkings</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="PARKING">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'un parking du réseau</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:complexType>
                                    <xsd:attribute name="id" type="xsd:string" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Identifiant du parking</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element minOccurs="0" name="REPARTITEURS">
                            <xsd:annotation>
                              <xsd:documentation>Liste des répartiteurs</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element minOccurs="0" maxOccurs="unbounded" name="REPARTITEUR"
                                  type="typeRepartiteur">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'un répartiteur</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element minOccurs="0" name="CONVERGENTS">
                            <xsd:annotation>
                              <xsd:documentation>Liste des convergents</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element minOccurs="0" maxOccurs="unbounded" name="CONVERGENT"
                                  type="typeConvergent">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'un convergent</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element minOccurs="0" name="GIRATOIRES">
                            <xsd:annotation>
                              <xsd:documentation>Liste des giratoires</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element minOccurs="0" maxOccurs="unbounded" name="GIRATOIRE"
                                  type="typeGiratoire">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'un répartiteur</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element minOccurs="0" name="CARREFOURSAFEUX">
                            <xsd:annotation>
                              <xsd:documentation>Liste des carrefours à feux</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element minOccurs="0" maxOccurs="unbounded"
                                  name="CARREFOURAFEUX" type="typeCarrefourAFeux">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'un carrefour à feux</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:all>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ZONES_DE_TERMINAISON">
                      <xsd:annotation>
                        <xsd:documentation>Liste des zones de terminaison</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" minOccurs="0"
                            name="ZONE_DE_TERMINAISON">
                            <xsd:annotation>
                              <xsd:documentation>Description d'une zone de terminaison</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:attribute name="id" type="xsd:string" use="required">
                                <xsd:annotation>
                                  <xsd:documentation>Identifiant de la zone de terminaison</xsd:documentation>
                                </xsd:annotation>
                              </xsd:attribute>
                              <xsd:attribute name="libelle" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation>Libellé de la zone de terminaison</xsd:documentation>
                                </xsd:annotation>
                              </xsd:attribute>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PARAMETRAGE_VEHICULES_GUIDES" type="typeLTG">
                      <xsd:annotation>
                        <xsd:documentation>Paramétrage des véhicules guidés du réseau</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PARAMETRAGE_LIVRAISONS"
                      type="typeFlotteLivraison"/>
                    <xsd:element minOccurs="0" name="ROUTES">
                      <xsd:annotation>
                        <xsd:documentation>Liste d'itinéraires du réseau</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" minOccurs="0" name="ROUTE">
                            <xsd:annotation>
                              <xsd:documentation>Description d'un itinéraire du réseau</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:choice maxOccurs="1">
                                <xsd:element name="TRONCONS_">
                                  <xsd:annotation>
                                    <xsd:documentation>Liste ordonnée des tronçons composant l'itinéraire</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:complexType>
                                    <xsd:sequence maxOccurs="1">
                                      <xsd:element maxOccurs="unbounded" name="TRONCON_"
                                        minOccurs="1">
                                        <xsd:annotation>
                                          <xsd:documentation>Tronçon de l'itinéraire</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                          <xsd:attribute use="required" name="id" type="xsd:string">
                                            <xsd:annotation>
                                              <xsd:documentation>Identifiant du tronçon</xsd:documentation>
                                            </xsd:annotation>
                                          </xsd:attribute>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="NOEUD">
                                  <xsd:annotation>
                                    <xsd:documentation>Noeud correspondant à l'itinéraire (cas par exemple d'un itinéraire de zone à zone en contact)</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:complexType>
                                    <xsd:attribute name="id" type="xsd:string" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Identifiant du noeud</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:choice>
                              <xsd:attribute name="id" type="xsd:ID" use="required">
                                <xsd:annotation>
                                  <xsd:documentation>Identifiant unique de l'itinéraire</xsd:documentation>
                                </xsd:annotation>
                              </xsd:attribute>
                              <xsd:attribute name="description" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation>Description de l'itinéraire</xsd:documentation>
                                </xsd:annotation>
                              </xsd:attribute>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="COORD_REFS">
                      <xsd:annotation>
                        <xsd:documentation>Liste des référentiels de coordonnées géographiques utilisés
</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                          <xsd:element name="COORD_REF">
                            <xsd:annotation>
                              <xsd:documentation>Description d'un référenciel géographiqe
</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence maxOccurs="unbounded">
                                <xsd:element minOccurs="0" name="COORD_MAP">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'un point du réseau dans le repère référence et dans le repère du réseau</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:complexType>
                                    <xsd:attribute name="internal" type="xsd:string" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Coordonnées réseau</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="external" type="xsd:string" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Coordonnées référence
</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                              <xsd:attribute name="application" type="xsd:string" use="optional">
                                <xsd:annotation>
                                  <xsd:documentation>Nom de l'application utilisant ce référentiel de coordonnées</xsd:documentation>
                                </xsd:annotation>
                              </xsd:attribute>
                              <xsd:attribute name="proj" type="xsd:string" use="optional">
                                <xsd:annotation>
                                  <xsd:documentation>Identifiant de la projection utilisé (ex : WGS84)</xsd:documentation>
                                </xsd:annotation>
                              </xsd:attribute>
                              <xsd:attribute name="north" type="xsd:float">
                                <xsd:annotation>
                                  <xsd:documentation>Angle de la zone de référence
</xsd:documentation>
                                </xsd:annotation>
                              </xsd:attribute>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="INIT">
                      <xsd:annotation>
                        <xsd:documentation>Initialisation du réseau par une liste de véhicule présent sur le réseau à l'instant 0</xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:sequence minOccurs="0" maxOccurs="unbounded">
                          <xsd:element name="TRAJS">
                            <xsd:annotation>
                              <xsd:documentation>Liste des véhicules présents à l'instant 0
						</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence maxOccurs="unbounded">
                                <xsd:element minOccurs="0" name="TRAJ">
                                  <xsd:annotation>
                                    <xsd:documentation>Description d'un véhicule présent sur le réseau à l'instant 0</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:complexType>
                                    <xsd:attribute name="id" type="xsd:positiveInteger"
                                      use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Identifiant du véhicule</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="type_veh" type="xsd:string" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Identifiant de la classe de véhicule</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="tron" type="xsd:string" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Tronçon sur lequel se trouve le véhicule (il peut ne pas être défini au niveau de la liste des tronçons notamment dans le cas où le véhicule est positionné sur un tronçon interne d'une brique. La vérification de l'existence du tronçon sera effectuée après l'initialisation complète du réseau par SymuVia. Si le tronçon n'existe pas, le véhicule ne sera pas créé.
								</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute default="1" name="voie"
                                      type="typeIntStrictPositif" use="optional">
                                      <xsd:annotation>
                                        <xsd:documentation>Numéro de voie sur laquelle se trouve le véhicule (la vérification de l'existence de la voie sera effectuée après l'initialisation complète du réseau par SymuVia. Si le tronçon n'existe pas, le véhicule ne sera pas créé.
								</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="dst" type="xsd:float" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Position curviligne du véhicule sur le tronçon à l'instant 0 (la vérification de la valeur de la position sera effectuée après l'initialisation complète du réseau par SymuVia. Si la position est erronée, le véhicule ne sera pas créé.
								</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="vit" type="typeFloatPositif" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Vitesse du véhicule à l'instant 0
								</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="acc" type="xsd:float" use="required">
                                      <xsd:annotation>
                                        <xsd:documentation>Accélération du véhicule à l'instant 0
								</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="destination" type="xsd:string"
                                      use="optional">
                                      <xsd:annotation>
                                        <xsd:documentation>Destination du véhicule (identifiant de la sortie). Cet attribut doit être obligatoirement présent dans le cas d'une simulation avec un flux de type 'destination' ou 'itineraire'.
								</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:attribute>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:all>
                  <xsd:attribute name="id" type="xsd:ID" use="required">
                    <xsd:annotation>
                      <xsd:documentation>Identifiant de la section RESEAU</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="SCENARIOS">
          <xsd:annotation>
            <xsd:documentation>Liste des définitions de scénarios</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element maxOccurs="unbounded" name="SCENARIO">
                <xsd:annotation>
                  <xsd:documentation>Définition d'un scénario à simuler.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:attribute name="id" type="xsd:string" use="required">
                    <xsd:annotation>
                      <xsd:documentation>identifiant du scénario</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="simulation_id" type="xsd:string" use="required">
                    <xsd:annotation>
                      <xsd:documentation>identifiant de la section SIMULATION associée</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="trafic_id" type="xsd:string" use="required">
                    <xsd:annotation>
                      <xsd:documentation>identifiant de la section TRAFIC associée</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="reseau_id" type="xsd:string" use="required">
                    <xsd:annotation>
                      <xsd:documentation>identifiant de la section RESEAU associée</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="dirout" type="xsd:string" use="optional">
                    <xsd:annotation>
                      <xsd:documentation>Répertoire de sortie des résultats de la simulation</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="prefout" type="xsd:string" use="required">
