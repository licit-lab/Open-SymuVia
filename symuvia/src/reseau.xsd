      <xsd:selector xpath=".//PARAMETRAGE_LIVRAISONS/POINTS_DE_LIVRAISON/POINT_DE_LIVRAISON"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="PositionTronconKey" refer="TronconKey">
      <xsd:selector xpath=".//POSITION_TRONCON"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:key name="LogistiqueODKey">
      <xsd:selector
        xpath=".//CONNEXIONS/EXTREMITES/EXTREMITE | .//CENTRES_LOGISTIQUES/CENTRE_LOGISTIQUE"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="TourneeOrigine" refer="LogistiqueODKey">
      <xsd:selector xpath=".//TOURNEE_DE_LIVRAISON/ORIGINE"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="TourneeDestination" refer="LogistiqueODKey">
      <xsd:selector xpath=".//TOURNEE_DE_LIVRAISON/DESTINATION"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:keyref name="PointDeLivraisonTourneeKey" refer="PointDeLivraisonKey">
      <xsd:selector xpath=".//TOURNEE_DE_LIVRAISON/POINTS_DE_LIVRAISON/POINT_DE_LIVRAISON"/>
      <xsd:field xpath="@id"/>
    </xsd:keyref>
    <xsd:key name="TourneeKey">
      <xsd:selector xpath=".//TOURNEE_DE_LIVRAISON"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:key name="MotifKey">
      <xsd:selector xpath="./TRAFICS/TRAFIC/MOTIFS/MOTIF"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="ZoneMotifOriginRepKey" refer="MotifKey">
      <xsd:selector xpath=".//MOTIF_ZONE"/>
      <xsd:field xpath="@motif_origine"/>
    </xsd:keyref>
    <xsd:keyref name="ZoneMotifDestinationRepKey" refer="MotifKey">
      <xsd:selector xpath=".//MOTIF_ZONE"/>
      <xsd:field xpath="@motif_destination"/>
    </xsd:keyref>
    <xsd:keyref name="ZonePlaqueMotifKey" refer="MotifKey">
      <xsd:selector
        xpath="./TRAFICS/TRAFIC/ZONES_DE_TERMINAISON/ZONE_DE_TERMINAISON/PLAQUES/PLAQUE/REP_MOTIFS_PLAQUES/REP_MOTIF_PLAQUE"/>
      <xsd:field xpath="@motif"/>
    </xsd:keyref>
    <xsd:keyref name="FluxRepMotifOrigin" refer="MotifKey">
      <xsd:selector
        xpath=".//FLUX_TYPEVEH/FLUX/REP_MOTIFS/REP_MOTIF/REP_MOTIF_DEST/MOTIF_COEFF"/>
      <xsd:field xpath="@motif_origine"/>
    </xsd:keyref>
    <xsd:keyref name="FluxRepMotifDst" refer="MotifKey">
      <xsd:selector
        xpath=".//FLUX/REP_MOTIFS/REP_MOTIF/REP_MOTIF_DEST/MOTIF_COEFF"/>
      <xsd:field xpath="@motif_destination"/>
    </xsd:keyref>
  </xsd:element>
  <xsd:complexType name="typeSensor" mixed="false">
    <xsd:all minOccurs="0">
      <xsd:element maxOccurs="1" minOccurs="0" name="PARAMETERS" type="typeAny">
        <xsd:annotation>
          <xsd:documentation>Tous autres paramètres spécifique à ce capteur.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="sensor_id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant du capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="sensor_type" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Type de capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="python_module" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Nom du module python correspondant le cas échéant</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ext_library_id" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la librairie externe à utiliser</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="update_frequency" type="xsd:positiveInteger" use="required">
      <xsd:annotation>
        <xsd:documentation>Frequence de l'appelle à la fonction de mise à jour (en ms).</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="break_down_rate" type="typeFloat01" use="required">
      <xsd:annotation>
        <xsd:documentation>Taux de panne du capteur (en %)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="precision" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation>Précision des résultats obtenus par le capteur. Les résultats seront précis a 10^i, dans l'unité de la variable calculé</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="noise_rate" type="typeFloat01" use="required">
      <xsd:annotation>
        <xsd:documentation>Taux de bruit des résultats du capteur</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="is_static_sensor" type="xsd:boolean" use="required">
      <xsd:annotation>
        <xsd:documentation>true si le capteur est un capteur statique. false sinon (capteur dynamique)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeApplication" mixed="false">
    <xsd:all minOccurs="0">
      <xsd:element maxOccurs="1" minOccurs="0" name="PARAMETERS" type="typeAny">
        <xsd:annotation>
          <xsd:documentation>Tous autres paramètres spécifique à cette application.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:all>
    <xsd:attribute name="application_id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de l'application</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="application_type" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Type d'application</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="python_module" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Nom du module python correspondant le cas échéant</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ext_library_id" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la librairie externe à utiliser</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="typeStationITS" mixed="false">
    <xsd:sequence minOccurs="1">
      <xsd:element maxOccurs="1" minOccurs="0" name="PARAMETERS" type="typeAny">
        <xsd:annotation>
          <xsd:documentation>Tous autres paramètres spécifique à cette station.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DYNAMIC_ELEMENTS">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="DYNAMIC_ELEMENT">
              <xsd:complexType>
                <xsd:attribute name="id_type_vehicule" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Type de véhicules qui doit être implémenté</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="equiped_rate" type="typeFloat01" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Taux de véhicules équipés</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FLUX_SENSORS">
        <xsd:annotation>
          <xsd:documentation>Liste des capteurs pour cette station ITS</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="1" minOccurs="1">
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="FLUX_SENSOR" nillable="false">
              <xsd:annotation>
                <xsd:documentation>Définition d'un capteur</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="sensor_id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Référence vers l'identifiant du capteur utilisé</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FLUX_APPLICATIONS">
        <xsd:annotation>
          <xsd:documentation>Liste des applications pour cette station ITS</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="FLUX_APPLICATION">
              <xsd:annotation>
                <xsd:documentation>Définition d'une station ITS</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="application_id" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation>Référence vers l'identifiant de l'application utilisé</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="station_id" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la station ITS</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="station_type" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Type de la station ITS</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="python_module" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Nom du module python correspondant le cas échéant</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ext_library_id" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Identifiant de la librairie externe à utiliser</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="range" type="xsd:double" use="optional" default="30">
      <xsd:annotation>
        <xsd:documentation>Portée de la station pour l'envoie de message.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="can_send" type="xsd:boolean" use="optional"
      default="true">
      <xsd:annotation>
        <xsd:documentation>Indique si la station est capable d'envoyer des messages.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="can_receive" type="xsd:boolean" use="optional"
      default="true">
      <xsd:annotation>
        <xsd:documentation>Indique si la station est capable de recevoir des messages.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="alpha_down" type="typeFloat01" use="optional"
      default="0.0">
      <xsd:annotation>
        <xsd:documentation>Probabilité pour la station de tomber en panne</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="beta_down" type="typeFloat01" use="optional"
      default="1.0">
      <xsd:annotation>
        <xsd:documentation>Probabilité pour la station de se rétablir après une panne</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="pos_x" type="xsd:double" use="optional">
      <xsd:annotation>
        <xsd:documentation>Coordonnée X de la position de l'entité dans le réseau</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="pos_y" type="xsd:double" use="optional">
      <xsd:annotation>
        <xsd:documentation>Coordonnée Y de la position de l'entité dans le réseau</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute form="unqualified" name="pos_z" type="xsd:double" use="optional">
      <xsd:annotation>
        <xsd:documentation>Coordonnée Z de la position de l'entité dans le réseau</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="SYMUCOM_ELEMENTS">
    <xsd:annotation>
      <xsd:documentation>Noeud racine des données d'entrée du système coopératif</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:all>
        <xsd:element name="REPARTITION">
          <xsd:annotation>
            <xsd:documentation>Définition des paramètres pour la répartition des entités connectées</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:attribute name="symucom_xml" type="xsd:string" use="required"/>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="SENSORS">
          <xsd:annotation>
            <xsd:documentation>Liste des définitions des paramètres des capteurs</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence minOccurs="1">
              <xsd:element name="SENSOR" maxOccurs="unbounded" minOccurs="0" nillable="false"
                type="typeSensor">
                <xsd:annotation>
                  <xsd:documentation>Paramètres d'un capteur</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="APPLICATIONS">
          <xsd:annotation>
            <xsd:documentation>Liste des définitions des paramètres des applications C-ITS</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence minOccurs="1">
              <xsd:element name="APPLICATION" maxOccurs="unbounded" minOccurs="0" nillable="false"
                type="typeApplication">
                <xsd:annotation>
                  <xsd:documentation>Paramètres d'une applications C-ITS</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="ITS_STATIONS">
          <xsd:annotation>
            <xsd:documentation>Liste des définitions des paramètres des stations ITS</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence minOccurs="1">
              <xsd:element name="ITS_STATION" maxOccurs="unbounded" minOccurs="0" nillable="false"
                type="typeStationITS">
                <xsd:annotation>
                  <xsd:documentation>Paramètres d'une station ITS</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element minOccurs="0" name="EXTERNAL_DYNAMIC_LIBRARIES">
          <xsd:annotation>
            <xsd:documentation>Liste des librairies dynamiques utilisables par les capteurs, stations et applications ITS</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element maxOccurs="unbounded" minOccurs="0" name="EXTERNAL_DYNAMIC_LIBRARY">
                <xsd:annotation>
                  <xsd:documentation>Définition d'une librairie externe à utiliser pour les applications ITS</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                  <xsd:attribute name="id" type="xsd:string" use="required">
                    <xsd:annotation>
                      <xsd:documentation>Identifiant de la librairie externe (pour référencement par les capteurs, stations et application qui l'utilisent)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                  <xsd:attribute name="module" type="xsd:string" use="required">
                    <xsd:annotation>
                      <xsd:documentation>Chemin de la librairie dynamique à utiliser (peut être le nom du fichier si la librairie est dans le même dossier que SymuVia)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:all>
    </xsd:complexType>
    <xsd:unique name="applicationLabelUnique">
      <xsd:selector xpath="./APPLICATIONS/APPLICATION"/>
      <xsd:field xpath="@application_id"/>
    </xsd:unique>
    <xsd:keyref name="application_idKeyref" refer="applicationLabelUnique">
      <xsd:selector xpath="./ITS_STATIONS/ITS_STATION/FLUX_APPLICATIONS/FLUX_APPLICATION"/>
      <xsd:field xpath="@application_id"/>
    </xsd:keyref>
    <xsd:unique name="sensorLabelUnique">
      <xsd:selector xpath="./SENSORS/SENSOR"/>
      <xsd:field xpath="@sensor_id"/>
    </xsd:unique>
    <xsd:keyref name="newKeyref" refer="sensorLabelUnique">
      <xsd:selector xpath="./ITS_STATIONS/ITS_STATION/FLUX_SENSORS/FLUX_SENSOR"/>
      <xsd:field xpath="@sensor_id"/>
    </xsd:keyref>
    <xsd:unique name="stationLabelUnique">
      <xsd:selector xpath="./ITS_STATIONS/ITS_STATION"/>
      <xsd:field xpath="@station_id"/>
    </xsd:unique>
    <xsd:key name="externalLibraryKey">
      <xsd:selector xpath="./EXTERNAL_DYNAMIC_LIBRARIES/EXTERNAL_DYNAMIC_LIBRARY"/>
      <xsd:field xpath="@id"/>
    </xsd:key>
    <xsd:keyref name="externalSensorKeyRef" refer="externalLibraryKey">
      <xsd:selector xpath="./SENSORS/SENSOR"/>
      <xsd:field xpath="@ext_library_id"/>
    </xsd:keyref>
    <xsd:keyref name="externalApplicationKeyRef" refer="externalLibraryKey">
      <xsd:selector xpath="./APPLICATIONS/APPLICATION"/>
      <xsd:field xpath="@ext_library_id"/>
    </xsd:keyref>
    <xsd:keyref name="externalStationKeyRef" refer="externalLibraryKey">
      <xsd:selector xpath="./ITS_STATIONS/ITS_STATION"/>
      <xsd:field xpath="@ext_library_id"/>
    </xsd:keyref>
  </xsd:element>
</xsd:schema>
